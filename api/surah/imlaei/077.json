{"surah":"77", "verses": [{"verse_key":"77:1","id":5623,"text":"وَالْمُرْسَلَاتِ عُرْفًا"},
{"verse_key":"77:2","id":5624,"text":"فَالْعَاصِفَاتِ عَصْفًا"},
{"verse_key":"77:3","id":5625,"text":"وَالنَّاشِرَاتِ نَشْرًا"},
{"verse_key":"77:4","id":5626,"text":"فَالْفَارِقَاتِ فَرْقًا"},
{"verse_key":"77:5","id":5627,"text":"فَالْمُلْقِيَاتِ ذِكْرًا"},
{"verse_key":"77:6","id":5628,"text":"عُذْرًا أَوْ نُذْرًا"},
{"verse_key":"77:7","id":5629,"text":"إِنَّمَا تُوعَدُونَ لَوَاقِعٌ"},
{"verse_key":"77:8","id":5630,"text":"فَإِذَا النُّجُومُ طُمِسَتْ"},
{"verse_key":"77:9","id":5631,"text":"وَإِذَا السَّمَاءُ فُرِجَتْ"},
{"verse_key":"77:10","id":5632,"text":"وَإِذَا الْجِبَالُ نُسِفَتْ"},
{"verse_key":"77:11","id":5633,"text":"وَإِذَا الرُّسُلُ أُقِّتَتْ"},
{"verse_key":"77:12","id":5634,"text":"لِأَيِّ يَوْمٍ أُجِّلَتْ"},
{"verse_key":"77:13","id":5635,"text":"لِيَوْمِ الْفَصْلِ"},
{"verse_key":"77:14","id":5636,"text":"وَمَا أَدْرَاكَ مَا يَوْمُ الْفَصْلِ"},
{"verse_key":"77:15","id":5637,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:16","id":5638,"text":"أَلَمْ نُهْلِكِ الْأَوَّلِينَ"},
{"verse_key":"77:17","id":5639,"text":"ثُمَّ نُتْبِعُهُمُ الْآخِرِينَ"},
{"verse_key":"77:18","id":5640,"text":"كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَ"},
{"verse_key":"77:19","id":5641,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:20","id":5642,"text":"أَلَمْ نَخْلُقكُّم مِّن مَّاءٍ مَّهِينٍ"},
{"verse_key":"77:21","id":5643,"text":"فَجَعَلْنَاهُ فِي قَرَارٍ مَّكِينٍ"},
{"verse_key":"77:22","id":5644,"text":"إِلَىٰ قَدَرٍ مَّعْلُومٍ"},
{"verse_key":"77:23","id":5645,"text":"فَقَدَرْنَا فَنِعْمَ الْقَادِرُونَ"},
{"verse_key":"77:24","id":5646,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:25","id":5647,"text":"أَلَمْ نَجْعَلِ الْأَرْضَ كِفَاتًا"},
{"verse_key":"77:26","id":5648,"text":"أَحْيَاءً وَأَمْوَاتًا"},
{"verse_key":"77:27","id":5649,"text":"وَجَعَلْنَا فِيهَا رَوَاسِيَ شَامِخَاتٍ وَأَسْقَيْنَاكُم مَّاءً فُرَاتًا"},
{"verse_key":"77:28","id":5650,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:29","id":5651,"text":"انطَلِقُوا إِلَىٰ مَا كُنتُم بِهِ تُكَذِّبُونَ"},
{"verse_key":"77:30","id":5652,"text":"انطَلِقُوا إِلَىٰ ظِلٍّ ذِي ثَلَاثِ شُعَبٍ"},
{"verse_key":"77:31","id":5653,"text":"لَّا ظَلِيلٍ وَلَا يُغْنِي مِنَ اللَّهَبِ"},
{"verse_key":"77:32","id":5654,"text":"إِنَّهَا تَرْمِي بِشَرَرٍ كَالْقَصْرِ"},
{"verse_key":"77:33","id":5655,"text":"كَأَنَّهُ جِمَالَتٌ صُفْرٌ"},
{"verse_key":"77:34","id":5656,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:35","id":5657,"text":"هَٰذَا يَوْمُ لَا يَنطِقُونَ"},
{"verse_key":"77:36","id":5658,"text":"وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ"},
{"verse_key":"77:37","id":5659,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:38","id":5660,"text":"هَٰذَا يَوْمُ الْفَصْلِ ۖ جَمَعْنَاكُمْ وَالْأَوَّلِينَ"},
{"verse_key":"77:39","id":5661,"text":"فَإِن كَانَ لَكُمْ كَيْدٌ فَكِيدُونِ"},
{"verse_key":"77:40","id":5662,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:41","id":5663,"text":"إِنَّ الْمُتَّقِينَ فِي ظِلَالٍ وَعُيُونٍ"},
{"verse_key":"77:42","id":5664,"text":"وَفَوَاكِهَ مِمَّا يَشْتَهُونَ"},
{"verse_key":"77:43","id":5665,"text":"كُلُوا وَاشْرَبُوا هَنِيئًا بِمَا كُنتُمْ تَعْمَلُونَ"},
{"verse_key":"77:44","id":5666,"text":"إِنَّا كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ"},
{"verse_key":"77:45","id":5667,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:46","id":5668,"text":"كُلُوا وَتَمَتَّعُوا قَلِيلًا إِنَّكُم مُّجْرِمُونَ"},
{"verse_key":"77:47","id":5669,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:48","id":5670,"text":"وَإِذَا قِيلَ لَهُمُ ارْكَعُوا لَا يَرْكَعُونَ"},
{"verse_key":"77:49","id":5671,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:50","id":5672,"text":"فَبِأَيِّ حَدِيثٍ بَعْدَهُ يُؤْمِنُونَ"}]}