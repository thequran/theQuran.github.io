{"surah":"83", "verses": [{"verse_key":"83:1","id":5849,"text":"وَيْلٌ لِّلْمُطَفِّفِينَ"},
{"verse_key":"83:2","id":5850,"text":"الَّذِينَ إِذَا اكْتَالُوا عَلَى النَّاسِ يَسْتَوْفُونَ"},
{"verse_key":"83:3","id":5851,"text":"وَإِذَا كَالُوهُمْ أَو وَّزَنُوهُمْ يُخْسِرُونَ"},
{"verse_key":"83:4","id":5852,"text":"أَلَا يَظُنُّ أُولَٰئِكَ أَنَّهُم مَّبْعُوثُونَ"},
{"verse_key":"83:5","id":5853,"text":"لِيَوْمٍ عَظِيمٍ"},
{"verse_key":"83:6","id":5854,"text":"يَوْمَ يَقُومُ النَّاسُ لِرَبِّ الْعَالَمِينَ"},
{"verse_key":"83:7","id":5855,"text":"كَلَّا إِنَّ كِتَابَ الْفُجَّارِ لَفِي سِجِّينٍ"},
{"verse_key":"83:8","id":5856,"text":"وَمَا أَدْرَاكَ مَا سِجِّينٌ"},
{"verse_key":"83:9","id":5857,"text":"كِتَابٌ مَّرْقُومٌ"},
{"verse_key":"83:10","id":5858,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"83:11","id":5859,"text":"الَّذِينَ يُكَذِّبُونَ بِيَوْمِ الدِّينِ"},
{"verse_key":"83:12","id":5860,"text":"وَمَا يُكَذِّبُ بِهِ إِلَّا كُلُّ مُعْتَدٍ أَثِيمٍ"},
{"verse_key":"83:13","id":5861,"text":"إِذَا تُتْلَىٰ عَلَيْهِ آيَاتُنَا قَالَ أَسَاطِيرُ الْأَوَّلِينَ"},
{"verse_key":"83:14","id":5862,"text":"كَلَّا ۖ بَلْ ۜ رَانَ عَلَىٰ قُلُوبِهِم مَّا كَانُوا يَكْسِبُونَ"},
{"verse_key":"83:15","id":5863,"text":"كَلَّا إِنَّهُمْ عَن رَّبِّهِمْ يَوْمَئِذٍ لَّمَحْجُوبُونَ"},
{"verse_key":"83:16","id":5864,"text":"ثُمَّ إِنَّهُمْ لَصَالُو الْجَحِيمِ"},
{"verse_key":"83:17","id":5865,"text":"ثُمَّ يُقَالُ هَٰذَا الَّذِي كُنتُم بِهِ تُكَذِّبُونَ"},
{"verse_key":"83:18","id":5866,"text":"كَلَّا إِنَّ كِتَابَ الْأَبْرَارِ لَفِي عِلِّيِّينَ"},
{"verse_key":"83:19","id":5867,"text":"وَمَا أَدْرَاكَ مَا عِلِّيُّونَ"},
{"verse_key":"83:20","id":5868,"text":"كِتَابٌ مَّرْقُومٌ"},
{"verse_key":"83:21","id":5869,"text":"يَشْهَدُهُ الْمُقَرَّبُونَ"},
{"verse_key":"83:22","id":5870,"text":"إِنَّ الْأَبْرَارَ لَفِي نَعِيمٍ"},
{"verse_key":"83:23","id":5871,"text":"عَلَى الْأَرَائِكِ يَنظُرُونَ"},
{"verse_key":"83:24","id":5872,"text":"تَعْرِفُ فِي وُجُوهِهِمْ نَضْرَةَ النَّعِيمِ"},
{"verse_key":"83:25","id":5873,"text":"يُسْقَوْنَ مِن رَّحِيقٍ مَّخْتُومٍ"},
{"verse_key":"83:26","id":5874,"text":"خِتَامُهُ مِسْكٌ ۚ وَفِي ذَٰلِكَ فَلْيَتَنَافَسِ الْمُتَنَافِسُونَ"},
{"verse_key":"83:27","id":5875,"text":"وَمِزَاجُهُ مِن تَسْنِيمٍ"},
{"verse_key":"83:28","id":5876,"text":"عَيْنًا يَشْرَبُ بِهَا الْمُقَرَّبُونَ"},
{"verse_key":"83:29","id":5877,"text":"إِنَّ الَّذِينَ أَجْرَمُوا كَانُوا مِنَ الَّذِينَ آمَنُوا يَضْحَكُونَ"},
{"verse_key":"83:30","id":5878,"text":"وَإِذَا مَرُّوا بِهِمْ يَتَغَامَزُونَ"},
{"verse_key":"83:31","id":5879,"text":"وَإِذَا انقَلَبُوا إِلَىٰ أَهْلِهِمُ انقَلَبُوا فَكِهِينَ"},
{"verse_key":"83:32","id":5880,"text":"وَإِذَا رَأَوْهُمْ قَالُوا إِنَّ هَٰؤُلَاءِ لَضَالُّونَ"},
{"verse_key":"83:33","id":5881,"text":"وَمَا أُرْسِلُوا عَلَيْهِمْ حَافِظِينَ"},
{"verse_key":"83:34","id":5882,"text":"فَالْيَوْمَ الَّذِينَ آمَنُوا مِنَ الْكُفَّارِ يَضْحَكُونَ"},
{"verse_key":"83:35","id":5883,"text":"عَلَى الْأَرَائِكِ يَنظُرُونَ"},
{"verse_key":"83:36","id":5884,"text":"هَلْ ثُوِّبَ الْكُفَّارُ مَا كَانُوا يَفْعَلُونَ"}]}