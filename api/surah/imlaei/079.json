{"surah":"79", "verses": [{"verse_key":"79:1","id":5713,"text":"وَالنَّازِعَاتِ غَرْقًا"},
{"verse_key":"79:2","id":5714,"text":"وَالنَّاشِطَاتِ نَشْطًا"},
{"verse_key":"79:3","id":5715,"text":"وَالسَّابِحَاتِ سَبْحًا"},
{"verse_key":"79:4","id":5716,"text":"فَالسَّابِقَاتِ سَبْقًا"},
{"verse_key":"79:5","id":5717,"text":"فَالْمُدَبِّرَاتِ أَمْرًا"},
{"verse_key":"79:6","id":5718,"text":"يَوْمَ تَرْجُفُ الرَّاجِفَةُ"},
{"verse_key":"79:7","id":5719,"text":"تَتْبَعُهَا الرَّادِفَةُ"},
{"verse_key":"79:8","id":5720,"text":"قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ"},
{"verse_key":"79:9","id":5721,"text":"أَبْصَارُهَا خَاشِعَةٌ"},
{"verse_key":"79:10","id":5722,"text":"يَقُولُونَ أَإِنَّا لَمَرْدُودُونَ فِي الْحَافِرَةِ"},
{"verse_key":"79:11","id":5723,"text":"أَإِذَا كُنَّا عِظَامًا نَّخِرَةً"},
{"verse_key":"79:12","id":5724,"text":"قَالُوا تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ"},
{"verse_key":"79:13","id":5725,"text":"فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ"},
{"verse_key":"79:14","id":5726,"text":"فَإِذَا هُم بِالسَّاهِرَةِ"},
{"verse_key":"79:15","id":5727,"text":"هَلْ أَتَاكَ حَدِيثُ مُوسَىٰ"},
{"verse_key":"79:16","id":5728,"text":"إِذْ نَادَاهُ رَبُّهُ بِالْوَادِ الْمُقَدَّسِ طُوًى"},
{"verse_key":"79:17","id":5729,"text":"اذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُ طَغَىٰ"},
{"verse_key":"79:18","id":5730,"text":"فَقُلْ هَل لَّكَ إِلَىٰ أَن تَزَكَّىٰ"},
{"verse_key":"79:19","id":5731,"text":"وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ"},
{"verse_key":"79:20","id":5732,"text":"فَأَرَاهُ الْآيَةَ الْكُبْرَىٰ"},
{"verse_key":"79:21","id":5733,"text":"فَكَذَّبَ وَعَصَىٰ"},
{"verse_key":"79:22","id":5734,"text":"ثُمَّ أَدْبَرَ يَسْعَىٰ"},
{"verse_key":"79:23","id":5735,"text":"فَحَشَرَ فَنَادَىٰ"},
{"verse_key":"79:24","id":5736,"text":"فَقَالَ أَنَا رَبُّكُمُ الْأَعْلَىٰ"},
{"verse_key":"79:25","id":5737,"text":"فَأَخَذَهُ اللَّهُ نَكَالَ الْآخِرَةِ وَالْأُولَىٰ"},
{"verse_key":"79:26","id":5738,"text":"إِنَّ فِي ذَٰلِكَ لَعِبْرَةً لِّمَن يَخْشَىٰ"},
{"verse_key":"79:27","id":5739,"text":"أَأَنتُمْ أَشَدُّ خَلْقًا أَمِ السَّمَاءُ ۚ بَنَاهَا"},
{"verse_key":"79:28","id":5740,"text":"رَفَعَ سَمْكَهَا فَسَوَّاهَا"},
{"verse_key":"79:29","id":5741,"text":"وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَاهَا"},
{"verse_key":"79:30","id":5742,"text":"وَالْأَرْضَ بَعْدَ ذَٰلِكَ دَحَاهَا"},
{"verse_key":"79:31","id":5743,"text":"أَخْرَجَ مِنْهَا مَاءَهَا وَمَرْعَاهَا"},
{"verse_key":"79:32","id":5744,"text":"وَالْجِبَالَ أَرْسَاهَا"},
{"verse_key":"79:33","id":5745,"text":"مَتَاعًا لَّكُمْ وَلِأَنْعَامِكُمْ"},
{"verse_key":"79:34","id":5746,"text":"فَإِذَا جَاءَتِ الطَّامَّةُ الْكُبْرَىٰ"},
{"verse_key":"79:35","id":5747,"text":"يَوْمَ يَتَذَكَّرُ الْإِنسَانُ مَا سَعَىٰ"},
{"verse_key":"79:36","id":5748,"text":"وَبُرِّزَتِ الْجَحِيمُ لِمَن يَرَىٰ"},
{"verse_key":"79:37","id":5749,"text":"فَأَمَّا مَن طَغَىٰ"},
{"verse_key":"79:38","id":5750,"text":"وَآثَرَ الْحَيَاةَ الدُّنْيَا"},
{"verse_key":"79:39","id":5751,"text":"فَإِنَّ الْجَحِيمَ هِيَ الْمَأْوَىٰ"},
{"verse_key":"79:40","id":5752,"text":"وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِ وَنَهَى النَّفْسَ عَنِ الْهَوَىٰ"},
{"verse_key":"79:41","id":5753,"text":"فَإِنَّ الْجَنَّةَ هِيَ الْمَأْوَىٰ"},
{"verse_key":"79:42","id":5754,"text":"يَسْأَلُونَكَ عَنِ السَّاعَةِ أَيَّانَ مُرْسَاهَا"},
{"verse_key":"79:43","id":5755,"text":"فِيمَ أَنتَ مِن ذِكْرَاهَا"},
{"verse_key":"79:44","id":5756,"text":"إِلَىٰ رَبِّكَ مُنتَهَاهَا"},
{"verse_key":"79:45","id":5757,"text":"إِنَّمَا أَنتَ مُنذِرُ مَن يَخْشَاهَا"},
{"verse_key":"79:46","id":5758,"text":"كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوا إِلَّا عَشِيَّةً أَوْ ضُحَاهَا"}]}