{"surah":"92", "verses": [{"verse_key":"92:1","id":6059,"text":"وَاللَّيْلِ إِذَا يَغْشَىٰ"},
{"verse_key":"92:2","id":6060,"text":"وَالنَّهَارِ إِذَا تَجَلَّىٰ"},
{"verse_key":"92:3","id":6061,"text":"وَمَا خَلَقَ الذَّكَرَ وَالْأُنثَىٰ"},
{"verse_key":"92:4","id":6062,"text":"إِنَّ سَعْيَكُمْ لَشَتَّىٰ"},
{"verse_key":"92:5","id":6063,"text":"فَأَمَّا مَنْ أَعْطَىٰ وَاتَّقَىٰ"},
{"verse_key":"92:6","id":6064,"text":"وَصَدَّقَ بِالْحُسْنَىٰ"},
{"verse_key":"92:7","id":6065,"text":"فَسَنُيَسِّرُهُ لِلْيُسْرَىٰ"},
{"verse_key":"92:8","id":6066,"text":"وَأَمَّا مَن بَخِلَ وَاسْتَغْنَىٰ"},
{"verse_key":"92:9","id":6067,"text":"وَكَذَّبَ بِالْحُسْنَىٰ"},
{"verse_key":"92:10","id":6068,"text":"فَسَنُيَسِّرُهُ لِلْعُسْرَىٰ"},
{"verse_key":"92:11","id":6069,"text":"وَمَا يُغْنِي عَنْهُ مَالُهُ إِذَا تَرَدَّىٰ"},
{"verse_key":"92:12","id":6070,"text":"إِنَّ عَلَيْنَا لَلْهُدَىٰ"},
{"verse_key":"92:13","id":6071,"text":"وَإِنَّ لَنَا لَلْآخِرَةَ وَالْأُولَىٰ"},
{"verse_key":"92:14","id":6072,"text":"فَأَنذَرْتُكُمْ نَارًا تَلَظَّىٰ"},
{"verse_key":"92:15","id":6073,"text":"لَا يَصْلَاهَا إِلَّا الْأَشْقَى"},
{"verse_key":"92:16","id":6074,"text":"الَّذِي كَذَّبَ وَتَوَلَّىٰ"},
{"verse_key":"92:17","id":6075,"text":"وَسَيُجَنَّبُهَا الْأَتْقَى"},
{"verse_key":"92:18","id":6076,"text":"الَّذِي يُؤْتِي مَالَهُ يَتَزَكَّىٰ"},
{"verse_key":"92:19","id":6077,"text":"وَمَا لِأَحَدٍ عِندَهُ مِن نِّعْمَةٍ تُجْزَىٰ"},
{"verse_key":"92:20","id":6078,"text":"إِلَّا ابْتِغَاءَ وَجْهِ رَبِّهِ الْأَعْلَىٰ"},
{"verse_key":"92:21","id":6079,"text":"وَلَسَوْفَ يَرْضَىٰ"}]}