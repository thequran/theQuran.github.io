{"surah":"80", "verses": [{"verse_key":"80:1","id":5759,"text":"عَبَسَ وَتَوَلَّىٰ"},
{"verse_key":"80:2","id":5760,"text":"أَن جَاءَهُ الْأَعْمَىٰ"},
{"verse_key":"80:3","id":5761,"text":"وَمَا يُدْرِيكَ لَعَلَّهُ يَزَّكَّىٰ"},
{"verse_key":"80:4","id":5762,"text":"أَوْ يَذَّكَّرُ فَتَنفَعَهُ الذِّكْرَىٰ"},
{"verse_key":"80:5","id":5763,"text":"أَمَّا مَنِ اسْتَغْنَىٰ"},
{"verse_key":"80:6","id":5764,"text":"فَأَنتَ لَهُ تَصَدَّىٰ"},
{"verse_key":"80:7","id":5765,"text":"وَمَا عَلَيْكَ أَلَّا يَزَّكَّىٰ"},
{"verse_key":"80:8","id":5766,"text":"وَأَمَّا مَن جَاءَكَ يَسْعَىٰ"},
{"verse_key":"80:9","id":5767,"text":"وَهُوَ يَخْشَىٰ"},
{"verse_key":"80:10","id":5768,"text":"فَأَنتَ عَنْهُ تَلَهَّىٰ"},
{"verse_key":"80:11","id":5769,"text":"كَلَّا إِنَّهَا تَذْكِرَةٌ"},
{"verse_key":"80:12","id":5770,"text":"فَمَن شَاءَ ذَكَرَهُ"},
{"verse_key":"80:13","id":5771,"text":"فِي صُحُفٍ مُّكَرَّمَةٍ"},
{"verse_key":"80:14","id":5772,"text":"مَّرْفُوعَةٍ مُّطَهَّرَةٍ"},
{"verse_key":"80:15","id":5773,"text":"بِأَيْدِي سَفَرَةٍ"},
{"verse_key":"80:16","id":5774,"text":"كِرَامٍ بَرَرَةٍ"},
{"verse_key":"80:17","id":5775,"text":"قُتِلَ الْإِنسَانُ مَا أَكْفَرَهُ"},
{"verse_key":"80:18","id":5776,"text":"مِنْ أَيِّ شَيْءٍ خَلَقَهُ"},
{"verse_key":"80:19","id":5777,"text":"مِن نُّطْفَةٍ خَلَقَهُ فَقَدَّرَهُ"},
{"verse_key":"80:20","id":5778,"text":"ثُمَّ السَّبِيلَ يَسَّرَهُ"},
{"verse_key":"80:21","id":5779,"text":"ثُمَّ أَمَاتَهُ فَأَقْبَرَهُ"},
{"verse_key":"80:22","id":5780,"text":"ثُمَّ إِذَا شَاءَ أَنشَرَهُ"},
{"verse_key":"80:23","id":5781,"text":"كَلَّا لَمَّا يَقْضِ مَا أَمَرَهُ"},
{"verse_key":"80:24","id":5782,"text":"فَلْيَنظُرِ الْإِنسَانُ إِلَىٰ طَعَامِهِ"},
{"verse_key":"80:25","id":5783,"text":"أَنَّا صَبَبْنَا الْمَاءَ صَبًّا"},
{"verse_key":"80:26","id":5784,"text":"ثُمَّ شَقَقْنَا الْأَرْضَ شَقًّا"},
{"verse_key":"80:27","id":5785,"text":"فَأَنبَتْنَا فِيهَا حَبًّا"},
{"verse_key":"80:28","id":5786,"text":"وَعِنَبًا وَقَضْبًا"},
{"verse_key":"80:29","id":5787,"text":"وَزَيْتُونًا وَنَخْلًا"},
{"verse_key":"80:30","id":5788,"text":"وَحَدَائِقَ غُلْبًا"},
{"verse_key":"80:31","id":5789,"text":"وَفَاكِهَةً وَأَبًّا"},
{"verse_key":"80:32","id":5790,"text":"مَّتَاعًا لَّكُمْ وَلِأَنْعَامِكُمْ"},
{"verse_key":"80:33","id":5791,"text":"فَإِذَا جَاءَتِ الصَّاخَّةُ"},
{"verse_key":"80:34","id":5792,"text":"يَوْمَ يَفِرُّ الْمَرْءُ مِنْ أَخِيهِ"},
{"verse_key":"80:35","id":5793,"text":"وَأُمِّهِ وَأَبِيهِ"},
{"verse_key":"80:36","id":5794,"text":"وَصَاحِبَتِهِ وَبَنِيهِ"},
{"verse_key":"80:37","id":5795,"text":"لِكُلِّ امْرِئٍ مِّنْهُمْ يَوْمَئِذٍ شَأْنٌ يُغْنِيهِ"},
{"verse_key":"80:38","id":5796,"text":"وُجُوهٌ يَوْمَئِذٍ مُّسْفِرَةٌ"},
{"verse_key":"80:39","id":5797,"text":"ضَاحِكَةٌ مُّسْتَبْشِرَةٌ"},
{"verse_key":"80:40","id":5798,"text":"وَوُجُوهٌ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌ"},
{"verse_key":"80:41","id":5799,"text":"تَرْهَقُهَا قَتَرَةٌ"},
{"verse_key":"80:42","id":5800,"text":"أُولَٰئِكَ هُمُ الْكَفَرَةُ الْفَجَرَةُ"}]}