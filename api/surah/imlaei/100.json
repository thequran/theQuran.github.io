{"surah":"100", "verses": [{"verse_key":"100:1","id":6147,"text":"وَالْعَادِيَاتِ ضَبْحًا"},
{"verse_key":"100:2","id":6148,"text":"فَالْمُورِيَاتِ قَدْحًا"},
{"verse_key":"100:3","id":6149,"text":"فَالْمُغِيرَاتِ صُبْحًا"},
{"verse_key":"100:4","id":6150,"text":"فَأَثَرْنَ بِهِ نَقْعًا"},
{"verse_key":"100:5","id":6151,"text":"فَوَسَطْنَ بِهِ جَمْعًا"},
{"verse_key":"100:6","id":6152,"text":"إِنَّ الْإِنسَانَ لِرَبِّهِ لَكَنُودٌ"},
{"verse_key":"100:7","id":6153,"text":"وَإِنَّهُ عَلَىٰ ذَٰلِكَ لَشَهِيدٌ"},
{"verse_key":"100:8","id":6154,"text":"وَإِنَّهُ لِحُبِّ الْخَيْرِ لَشَدِيدٌ"},
{"verse_key":"100:9","id":6155,"text":"أَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِي الْقُبُورِ"},
{"verse_key":"100:10","id":6156,"text":"وَحُصِّلَ مَا فِي الصُّدُورِ"},
{"verse_key":"100:11","id":6157,"text":"إِنَّ رَبَّهُم بِهِمْ يَوْمَئِذٍ لَّخَبِيرٌ"}]}