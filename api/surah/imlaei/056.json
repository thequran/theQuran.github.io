{"surah":"56", "verses": [{"verse_key":"56:1","id":4980,"text":"إِذَا وَقَعَتِ الْوَاقِعَةُ"},
{"verse_key":"56:2","id":4981,"text":"لَيْسَ لِوَقْعَتِهَا كَاذِبَةٌ"},
{"verse_key":"56:3","id":4982,"text":"خَافِضَةٌ رَّافِعَةٌ"},
{"verse_key":"56:4","id":4983,"text":"إِذَا رُجَّتِ الْأَرْضُ رَجًّا"},
{"verse_key":"56:5","id":4984,"text":"وَبُسَّتِ الْجِبَالُ بَسًّا"},
{"verse_key":"56:6","id":4985,"text":"فَكَانَتْ هَبَاءً مُّنبَثًّا"},
{"verse_key":"56:7","id":4986,"text":"وَكُنتُمْ أَزْوَاجًا ثَلَاثَةً"},
{"verse_key":"56:8","id":4987,"text":"فَأَصْحَابُ الْمَيْمَنَةِ مَا أَصْحَابُ الْمَيْمَنَةِ"},
{"verse_key":"56:9","id":4988,"text":"وَأَصْحَابُ الْمَشْأَمَةِ مَا أَصْحَابُ الْمَشْأَمَةِ"},
{"verse_key":"56:10","id":4989,"text":"وَالسَّابِقُونَ السَّابِقُونَ"},
{"verse_key":"56:11","id":4990,"text":"أُولَٰئِكَ الْمُقَرَّبُونَ"},
{"verse_key":"56:12","id":4991,"text":"فِي جَنَّاتِ النَّعِيمِ"},
{"verse_key":"56:13","id":4992,"text":"ثُلَّةٌ مِّنَ الْأَوَّلِينَ"},
{"verse_key":"56:14","id":4993,"text":"وَقَلِيلٌ مِّنَ الْآخِرِينَ"},
{"verse_key":"56:15","id":4994,"text":"عَلَىٰ سُرُرٍ مَّوْضُونَةٍ"},
{"verse_key":"56:16","id":4995,"text":"مُّتَّكِئِينَ عَلَيْهَا مُتَقَابِلِينَ"},
{"verse_key":"56:17","id":4996,"text":"يَطُوفُ عَلَيْهِمْ وِلْدَانٌ مُّخَلَّدُونَ"},
{"verse_key":"56:18","id":4997,"text":"بِأَكْوَابٍ وَأَبَارِيقَ وَكَأْسٍ مِّن مَّعِينٍ"},
{"verse_key":"56:19","id":4998,"text":"لَّا يُصَدَّعُونَ عَنْهَا وَلَا يُنزِفُونَ"},
{"verse_key":"56:20","id":4999,"text":"وَفَاكِهَةٍ مِّمَّا يَتَخَيَّرُونَ"},
{"verse_key":"56:21","id":5000,"text":"وَلَحْمِ طَيْرٍ مِّمَّا يَشْتَهُونَ"},
{"verse_key":"56:22","id":5001,"text":"وَحُورٌ عِينٌ"},
{"verse_key":"56:23","id":5002,"text":"كَأَمْثَالِ اللُّؤْلُؤِ الْمَكْنُونِ"},
{"verse_key":"56:24","id":5003,"text":"جَزَاءً بِمَا كَانُوا يَعْمَلُونَ"},
{"verse_key":"56:25","id":5004,"text":"لَا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا تَأْثِيمًا"},
{"verse_key":"56:26","id":5005,"text":"إِلَّا قِيلًا سَلَامًا سَلَامًا"},
{"verse_key":"56:27","id":5006,"text":"وَأَصْحَابُ الْيَمِينِ مَا أَصْحَابُ الْيَمِينِ"},
{"verse_key":"56:28","id":5007,"text":"فِي سِدْرٍ مَّخْضُودٍ"},
{"verse_key":"56:29","id":5008,"text":"وَطَلْحٍ مَّنضُودٍ"},
{"verse_key":"56:30","id":5009,"text":"وَظِلٍّ مَّمْدُودٍ"},
{"verse_key":"56:31","id":5010,"text":"وَمَاءٍ مَّسْكُوبٍ"},
{"verse_key":"56:32","id":5011,"text":"وَفَاكِهَةٍ كَثِيرَةٍ"},
{"verse_key":"56:33","id":5012,"text":"لَّا مَقْطُوعَةٍ وَلَا مَمْنُوعَةٍ"},
{"verse_key":"56:34","id":5013,"text":"وَفُرُشٍ مَّرْفُوعَةٍ"},
{"verse_key":"56:35","id":5014,"text":"إِنَّا أَنشَأْنَاهُنَّ إِنشَاءً"},
{"verse_key":"56:36","id":5015,"text":"فَجَعَلْنَاهُنَّ أَبْكَارًا"},
{"verse_key":"56:37","id":5016,"text":"عُرُبًا أَتْرَابًا"},
{"verse_key":"56:38","id":5017,"text":"لِّأَصْحَابِ الْيَمِينِ"},
{"verse_key":"56:39","id":5018,"text":"ثُلَّةٌ مِّنَ الْأَوَّلِينَ"},
{"verse_key":"56:40","id":5019,"text":"وَثُلَّةٌ مِّنَ الْآخِرِينَ"},
{"verse_key":"56:41","id":5020,"text":"وَأَصْحَابُ الشِّمَالِ مَا أَصْحَابُ الشِّمَالِ"},
{"verse_key":"56:42","id":5021,"text":"فِي سَمُومٍ وَحَمِيمٍ"},
{"verse_key":"56:43","id":5022,"text":"وَظِلٍّ مِّن يَحْمُومٍ"},
{"verse_key":"56:44","id":5023,"text":"لَّا بَارِدٍ وَلَا كَرِيمٍ"},
{"verse_key":"56:45","id":5024,"text":"إِنَّهُمْ كَانُوا قَبْلَ ذَٰلِكَ مُتْرَفِينَ"},
{"verse_key":"56:46","id":5025,"text":"وَكَانُوا يُصِرُّونَ عَلَى الْحِنثِ الْعَظِيمِ"},
{"verse_key":"56:47","id":5026,"text":"وَكَانُوا يَقُولُونَ أَئِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَامًا أَإِنَّا لَمَبْعُوثُونَ"},
{"verse_key":"56:48","id":5027,"text":"أَوَآبَاؤُنَا الْأَوَّلُونَ"},
{"verse_key":"56:49","id":5028,"text":"قُلْ إِنَّ الْأَوَّلِينَ وَالْآخِرِينَ"},
{"verse_key":"56:50","id":5029,"text":"لَمَجْمُوعُونَ إِلَىٰ مِيقَاتِ يَوْمٍ مَّعْلُومٍ"},
{"verse_key":"56:51","id":5030,"text":"ثُمَّ إِنَّكُمْ أَيُّهَا الضَّالُّونَ الْمُكَذِّبُونَ"},
{"verse_key":"56:52","id":5031,"text":"لَآكِلُونَ مِن شَجَرٍ مِّن زَقُّومٍ"},
{"verse_key":"56:53","id":5032,"text":"فَمَالِئُونَ مِنْهَا الْبُطُونَ"},
{"verse_key":"56:54","id":5033,"text":"فَشَارِبُونَ عَلَيْهِ مِنَ الْحَمِيمِ"},
{"verse_key":"56:55","id":5034,"text":"فَشَارِبُونَ شُرْبَ الْهِيمِ"},
{"verse_key":"56:56","id":5035,"text":"هَٰذَا نُزُلُهُمْ يَوْمَ الدِّينِ"},
{"verse_key":"56:57","id":5036,"text":"نَحْنُ خَلَقْنَاكُمْ فَلَوْلَا تُصَدِّقُونَ"},
{"verse_key":"56:58","id":5037,"text":"أَفَرَأَيْتُم مَّا تُمْنُونَ"},
{"verse_key":"56:59","id":5038,"text":"أَأَنتُمْ تَخْلُقُونَهُ أَمْ نَحْنُ الْخَالِقُونَ"},
{"verse_key":"56:60","id":5039,"text":"نَحْنُ قَدَّرْنَا بَيْنَكُمُ الْمَوْتَ وَمَا نَحْنُ بِمَسْبُوقِينَ"},
{"verse_key":"56:61","id":5040,"text":"عَلَىٰ أَن نُّبَدِّلَ أَمْثَالَكُمْ وَنُنشِئَكُمْ فِي مَا لَا تَعْلَمُونَ"},
{"verse_key":"56:62","id":5041,"text":"وَلَقَدْ عَلِمْتُمُ النَّشْأَةَ الْأُولَىٰ فَلَوْلَا تَذَكَّرُونَ"},
{"verse_key":"56:63","id":5042,"text":"أَفَرَأَيْتُم مَّا تَحْرُثُونَ"},
{"verse_key":"56:64","id":5043,"text":"أَأَنتُمْ تَزْرَعُونَهُ أَمْ نَحْنُ الزَّارِعُونَ"},
{"verse_key":"56:65","id":5044,"text":"لَوْ نَشَاءُ لَجَعَلْنَاهُ حُطَامًا فَظَلْتُمْ تَفَكَّهُونَ"},
{"verse_key":"56:66","id":5045,"text":"إِنَّا لَمُغْرَمُونَ"},
{"verse_key":"56:67","id":5046,"text":"بَلْ نَحْنُ مَحْرُومُونَ"},
{"verse_key":"56:68","id":5047,"text":"أَفَرَأَيْتُمُ الْمَاءَ الَّذِي تَشْرَبُونَ"},
{"verse_key":"56:69","id":5048,"text":"أَأَنتُمْ أَنزَلْتُمُوهُ مِنَ الْمُزْنِ أَمْ نَحْنُ الْمُنزِلُونَ"},
{"verse_key":"56:70","id":5049,"text":"لَوْ نَشَاءُ جَعَلْنَاهُ أُجَاجًا فَلَوْلَا تَشْكُرُونَ"},
{"verse_key":"56:71","id":5050,"text":"أَفَرَأَيْتُمُ النَّارَ الَّتِي تُورُونَ"},
{"verse_key":"56:72","id":5051,"text":"أَأَنتُمْ أَنشَأْتُمْ شَجَرَتَهَا أَمْ نَحْنُ الْمُنشِئُونَ"},
{"verse_key":"56:73","id":5052,"text":"نَحْنُ جَعَلْنَاهَا تَذْكِرَةً وَمَتَاعًا لِّلْمُقْوِينَ"},
{"verse_key":"56:74","id":5053,"text":"فَسَبِّحْ بِاسْمِ رَبِّكَ الْعَظِيمِ"},
{"verse_key":"56:75","id":5054,"text":"فَلَا أُقْسِمُ بِمَوَاقِعِ النُّجُومِ"},
{"verse_key":"56:76","id":5055,"text":"وَإِنَّهُ لَقَسَمٌ لَّوْ تَعْلَمُونَ عَظِيمٌ"},
{"verse_key":"56:77","id":5056,"text":"إِنَّهُ لَقُرْآنٌ كَرِيمٌ"},
{"verse_key":"56:78","id":5057,"text":"فِي كِتَابٍ مَّكْنُونٍ"},
{"verse_key":"56:79","id":5058,"text":"لَّا يَمَسُّهُ إِلَّا الْمُطَهَّرُونَ"},
{"verse_key":"56:80","id":5059,"text":"تَنزِيلٌ مِّن رَّبِّ الْعَالَمِينَ"},
{"verse_key":"56:81","id":5060,"text":"أَفَبِهَٰذَا الْحَدِيثِ أَنتُم مُّدْهِنُونَ"},
{"verse_key":"56:82","id":5061,"text":"وَتَجْعَلُونَ رِزْقَكُمْ أَنَّكُمْ تُكَذِّبُونَ"},
{"verse_key":"56:83","id":5062,"text":"فَلَوْلَا إِذَا بَلَغَتِ الْحُلْقُومَ"},
{"verse_key":"56:84","id":5063,"text":"وَأَنتُمْ حِينَئِذٍ تَنظُرُونَ"},
{"verse_key":"56:85","id":5064,"text":"وَنَحْنُ أَقْرَبُ إِلَيْهِ مِنكُمْ وَلَٰكِن لَّا تُبْصِرُونَ"},
{"verse_key":"56:86","id":5065,"text":"فَلَوْلَا إِن كُنتُمْ غَيْرَ مَدِينِينَ"},
{"verse_key":"56:87","id":5066,"text":"تَرْجِعُونَهَا إِن كُنتُمْ صَادِقِينَ"},
{"verse_key":"56:88","id":5067,"text":"فَأَمَّا إِن كَانَ مِنَ الْمُقَرَّبِينَ"},
{"verse_key":"56:89","id":5068,"text":"فَرَوْحٌ وَرَيْحَانٌ وَجَنَّتُ نَعِيمٍ"},
{"verse_key":"56:90","id":5069,"text":"وَأَمَّا إِن كَانَ مِنْ أَصْحَابِ الْيَمِينِ"},
{"verse_key":"56:91","id":5070,"text":"فَسَلَامٌ لَّكَ مِنْ أَصْحَابِ الْيَمِينِ"},
{"verse_key":"56:92","id":5071,"text":"وَأَمَّا إِن كَانَ مِنَ الْمُكَذِّبِينَ الضَّالِّينَ"},
{"verse_key":"56:93","id":5072,"text":"فَنُزُلٌ مِّنْ حَمِيمٍ"},
{"verse_key":"56:94","id":5073,"text":"وَتَصْلِيَةُ جَحِيمٍ"},
{"verse_key":"56:95","id":5074,"text":"إِنَّ هَٰذَا لَهُوَ حَقُّ الْيَقِينِ"},
{"verse_key":"56:96","id":5075,"text":"فَسَبِّحْ بِاسْمِ رَبِّكَ الْعَظِيمِ"}]}