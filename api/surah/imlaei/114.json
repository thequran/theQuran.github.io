      "id": 6234,
      "text": "مِن شَرِّ الْوَسْوَاسِ الْخَنَّاسِ"
    },
    {
      "verse_key": "114:5",
      "id": 6235,
      "text": "الَّذِي يُوَسْوِسُ فِي صُدُورِ النَّاسِ"
    },
    { "verse_key": "114:6", "id": 6236, "text": "مِنَ الْجِنَّةِ وَالنَّاسِ" }
