{"surah":"89", "verses": [{"verse_key":"89:1","id":5994,"text":"وَالْفَجْرِ"},
{"verse_key":"89:2","id":5995,"text":"وَلَيَالٍ عَشْرٍ"},
{"verse_key":"89:3","id":5996,"text":"وَالشَّفْعِ وَالْوَتْرِ"},
{"verse_key":"89:4","id":5997,"text":"وَاللَّيْلِ إِذَا يَسْرِ"},
{"verse_key":"89:5","id":5998,"text":"هَلْ فِي ذَٰلِكَ قَسَمٌ لِّذِي حِجْرٍ"},
{"verse_key":"89:6","id":5999,"text":"أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ"},
{"verse_key":"89:7","id":6000,"text":"إِرَمَ ذَاتِ الْعِمَادِ"},
{"verse_key":"89:8","id":6001,"text":"الَّتِي لَمْ يُخْلَقْ مِثْلُهَا فِي الْبِلَادِ"},
{"verse_key":"89:9","id":6002,"text":"وَثَمُودَ الَّذِينَ جَابُوا الصَّخْرَ بِالْوَادِ"},
{"verse_key":"89:10","id":6003,"text":"وَفِرْعَوْنَ ذِي الْأَوْتَادِ"},
{"verse_key":"89:11","id":6004,"text":"الَّذِينَ طَغَوْا فِي الْبِلَادِ"},
{"verse_key":"89:12","id":6005,"text":"فَأَكْثَرُوا فِيهَا الْفَسَادَ"},
{"verse_key":"89:13","id":6006,"text":"فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ"},
{"verse_key":"89:14","id":6007,"text":"إِنَّ رَبَّكَ لَبِالْمِرْصَادِ"},
{"verse_key":"89:15","id":6008,"text":"فَأَمَّا الْإِنسَانُ إِذَا مَا ابْتَلَاهُ رَبُّهُ فَأَكْرَمَهُ وَنَعَّمَهُ فَيَقُولُ رَبِّي أَكْرَمَنِ"},
{"verse_key":"89:16","id":6009,"text":"وَأَمَّا إِذَا مَا ابْتَلَاهُ فَقَدَرَ عَلَيْهِ رِزْقَهُ فَيَقُولُ رَبِّي أَهَانَنِ"},
{"verse_key":"89:17","id":6010,"text":"كَلَّا ۖ بَل لَّا تُكْرِمُونَ الْيَتِيمَ"},
{"verse_key":"89:18","id":6011,"text":"وَلَا تَحَاضُّونَ عَلَىٰ طَعَامِ الْمِسْكِينِ"},
{"verse_key":"89:19","id":6012,"text":"وَتَأْكُلُونَ التُّرَاثَ أَكْلًا لَّمًّا"},
{"verse_key":"89:20","id":6013,"text":"وَتُحِبُّونَ الْمَالَ حُبًّا جَمًّا"},
{"verse_key":"89:21","id":6014,"text":"كَلَّا إِذَا دُكَّتِ الْأَرْضُ دَكًّا دَكًّا"},
{"verse_key":"89:22","id":6015,"text":"وَجَاءَ رَبُّكَ وَالْمَلَكُ صَفًّا صَفًّا"},
{"verse_key":"89:23","id":6016,"text":"وَجِيءَ يَوْمَئِذٍ بِجَهَنَّمَ ۚ يَوْمَئِذٍ يَتَذَكَّرُ الْإِنسَانُ وَأَنَّىٰ لَهُ الذِّكْرَىٰ"},
{"verse_key":"89:24","id":6017,"text":"يَقُولُ يَا لَيْتَنِي قَدَّمْتُ لِحَيَاتِي"},
{"verse_key":"89:25","id":6018,"text":"فَيَوْمَئِذٍ لَّا يُعَذِّبُ عَذَابَهُ أَحَدٌ"},
{"verse_key":"89:26","id":6019,"text":"وَلَا يُوثِقُ وَثَاقَهُ أَحَدٌ"},
{"verse_key":"89:27","id":6020,"text":"يَا أَيَّتُهَا النَّفْسُ الْمُطْمَئِنَّةُ"},
{"verse_key":"89:28","id":6021,"text":"ارْجِعِي إِلَىٰ رَبِّكِ رَاضِيَةً مَّرْضِيَّةً"},
{"verse_key":"89:29","id":6022,"text":"فَادْخُلِي فِي عِبَادِي"},
{"verse_key":"89:30","id":6023,"text":"وَادْخُلِي جَنَّتِي"}]}