{"surah":"55", "verses": [{"verse_key":"55:1","id":4902,"text":"الرَّحْمَٰنُ"},
{"verse_key":"55:2","id":4903,"text":"عَلَّمَ الْقُرْآنَ"},
{"verse_key":"55:3","id":4904,"text":"خَلَقَ الْإِنسَانَ"},
{"verse_key":"55:4","id":4905,"text":"عَلَّمَهُ الْبَيَانَ"},
{"verse_key":"55:5","id":4906,"text":"الشَّمْسُ وَالْقَمَرُ بِحُسْبَانٍ"},
{"verse_key":"55:6","id":4907,"text":"وَالنَّجْمُ وَالشَّجَرُ يَسْجُدَانِ"},
{"verse_key":"55:7","id":4908,"text":"وَالسَّمَاءَ رَفَعَهَا وَوَضَعَ الْمِيزَانَ"},
{"verse_key":"55:8","id":4909,"text":"أَلَّا تَطْغَوْا فِي الْمِيزَانِ"},
{"verse_key":"55:9","id":4910,"text":"وَأَقِيمُوا الْوَزْنَ بِالْقِسْطِ وَلَا تُخْسِرُوا الْمِيزَانَ"},
{"verse_key":"55:10","id":4911,"text":"وَالْأَرْضَ وَضَعَهَا لِلْأَنَامِ"},
{"verse_key":"55:11","id":4912,"text":"فِيهَا فَاكِهَةٌ وَالنَّخْلُ ذَاتُ الْأَكْمَامِ"},
{"verse_key":"55:12","id":4913,"text":"وَالْحَبُّ ذُو الْعَصْفِ وَالرَّيْحَانُ"},
{"verse_key":"55:13","id":4914,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:14","id":4915,"text":"خَلَقَ الْإِنسَانَ مِن صَلْصَالٍ كَالْفَخَّارِ"},
{"verse_key":"55:15","id":4916,"text":"وَخَلَقَ الْجَانَّ مِن مَّارِجٍ مِّن نَّارٍ"},
{"verse_key":"55:16","id":4917,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:17","id":4918,"text":"رَبُّ الْمَشْرِقَيْنِ وَرَبُّ الْمَغْرِبَيْنِ"},
{"verse_key":"55:18","id":4919,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:19","id":4920,"text":"مَرَجَ الْبَحْرَيْنِ يَلْتَقِيَانِ"},
{"verse_key":"55:20","id":4921,"text":"بَيْنَهُمَا بَرْزَخٌ لَّا يَبْغِيَانِ"},
{"verse_key":"55:21","id":4922,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:22","id":4923,"text":"يَخْرُجُ مِنْهُمَا اللُّؤْلُؤُ وَالْمَرْجَانُ"},
{"verse_key":"55:23","id":4924,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:24","id":4925,"text":"وَلَهُ الْجَوَارِ الْمُنشَآتُ فِي الْبَحْرِ كَالْأَعْلَامِ"},
{"verse_key":"55:25","id":4926,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:26","id":4927,"text":"كُلُّ مَنْ عَلَيْهَا فَانٍ"},
{"verse_key":"55:27","id":4928,"text":"وَيَبْقَىٰ وَجْهُ رَبِّكَ ذُو الْجَلَالِ وَالْإِكْرَامِ"},
{"verse_key":"55:28","id":4929,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:29","id":4930,"text":"يَسْأَلُهُ مَن فِي السَّمَاوَاتِ وَالْأَرْضِ ۚ كُلَّ يَوْمٍ هُوَ فِي شَأْنٍ"},
{"verse_key":"55:30","id":4931,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:31","id":4932,"text":"سَنَفْرُغُ لَكُمْ أَيُّهَ الثَّقَلَانِ"},
{"verse_key":"55:32","id":4933,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:33","id":4934,"text":"يَا مَعْشَرَ الْجِنِّ وَالْإِنسِ إِنِ اسْتَطَعْتُمْ أَن تَنفُذُوا مِنْ أَقْطَارِ السَّمَاوَاتِ وَالْأَرْضِ فَانفُذُوا ۚ لَا تَنفُذُونَ إِلَّا بِسُلْطَانٍ"},
{"verse_key":"55:34","id":4935,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:35","id":4936,"text":"يُرْسَلُ عَلَيْكُمَا شُوَاظٌ مِّن نَّارٍ وَنُحَاسٌ فَلَا تَنتَصِرَانِ"},
{"verse_key":"55:36","id":4937,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:37","id":4938,"text":"فَإِذَا انشَقَّتِ السَّمَاءُ فَكَانَتْ وَرْدَةً كَالدِّهَانِ"},
{"verse_key":"55:38","id":4939,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:39","id":4940,"text":"فَيَوْمَئِذٍ لَّا يُسْأَلُ عَن ذَنبِهِ إِنسٌ وَلَا جَانٌّ"},
{"verse_key":"55:40","id":4941,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:41","id":4942,"text":"يُعْرَفُ الْمُجْرِمُونَ بِسِيمَاهُمْ فَيُؤْخَذُ بِالنَّوَاصِي وَالْأَقْدَامِ"},
{"verse_key":"55:42","id":4943,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:43","id":4944,"text":"هَٰذِهِ جَهَنَّمُ الَّتِي يُكَذِّبُ بِهَا الْمُجْرِمُونَ"},
{"verse_key":"55:44","id":4945,"text":"يَطُوفُونَ بَيْنَهَا وَبَيْنَ حَمِيمٍ آنٍ"},
{"verse_key":"55:45","id":4946,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:46","id":4947,"text":"وَلِمَنْ خَافَ مَقَامَ رَبِّهِ جَنَّتَانِ"},
{"verse_key":"55:47","id":4948,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:48","id":4949,"text":"ذَوَاتَا أَفْنَانٍ"},
{"verse_key":"55:49","id":4950,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:50","id":4951,"text":"فِيهِمَا عَيْنَانِ تَجْرِيَانِ"},
{"verse_key":"55:51","id":4952,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:52","id":4953,"text":"فِيهِمَا مِن كُلِّ فَاكِهَةٍ زَوْجَانِ"},
{"verse_key":"55:53","id":4954,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:54","id":4955,"text":"مُتَّكِئِينَ عَلَىٰ فُرُشٍ بَطَائِنُهَا مِنْ إِسْتَبْرَقٍ ۚ وَجَنَى الْجَنَّتَيْنِ دَانٍ"},
{"verse_key":"55:55","id":4956,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:56","id":4957,"text":"فِيهِنَّ قَاصِرَاتُ الطَّرْفِ لَمْ يَطْمِثْهُنَّ إِنسٌ قَبْلَهُمْ وَلَا جَانٌّ"},
{"verse_key":"55:57","id":4958,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:58","id":4959,"text":"كَأَنَّهُنَّ الْيَاقُوتُ وَالْمَرْجَانُ"},
{"verse_key":"55:59","id":4960,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:60","id":4961,"text":"هَلْ جَزَاءُ الْإِحْسَانِ إِلَّا الْإِحْسَانُ"},
{"verse_key":"55:61","id":4962,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:62","id":4963,"text":"وَمِن دُونِهِمَا جَنَّتَانِ"},
{"verse_key":"55:63","id":4964,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:64","id":4965,"text":"مُدْهَامَّتَانِ"},
{"verse_key":"55:65","id":4966,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:66","id":4967,"text":"فِيهِمَا عَيْنَانِ نَضَّاخَتَانِ"},
{"verse_key":"55:67","id":4968,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:68","id":4969,"text":"فِيهِمَا فَاكِهَةٌ وَنَخْلٌ وَرُمَّانٌ"},
{"verse_key":"55:69","id":4970,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:70","id":4971,"text":"فِيهِنَّ خَيْرَاتٌ حِسَانٌ"},
{"verse_key":"55:71","id":4972,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:72","id":4973,"text":"حُورٌ مَّقْصُورَاتٌ فِي الْخِيَامِ"},
{"verse_key":"55:73","id":4974,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:74","id":4975,"text":"لَمْ يَطْمِثْهُنَّ إِنسٌ قَبْلَهُمْ وَلَا جَانٌّ"},
{"verse_key":"55:75","id":4976,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:76","id":4977,"text":"مُتَّكِئِينَ عَلَىٰ رَفْرَفٍ خُضْرٍ وَعَبْقَرِيٍّ حِسَانٍ"},
{"verse_key":"55:77","id":4978,"text":"فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"},
{"verse_key":"55:78","id":4979,"text":"تَبَارَكَ اسْمُ رَبِّكَ ذِي الْجَلَالِ وَالْإِكْرَامِ"}]}