{"surah":"92", "verses": [{"verse_key":"92:1","id":6059,"text":"وَالَّيۡلِ اِذَا يَغۡشٰىۙ\u200F"},
{"verse_key":"92:2","id":6060,"text":"وَالنَّهَارِ اِذَا تَجَلّٰىۙ\u200F"},
{"verse_key":"92:3","id":6061,"text":"وَمَا خَلَقَ الذَّكَرَ وَالۡاُنۡثٰٓىۙ\u200F"},
{"verse_key":"92:4","id":6062,"text":"اِنَّ سَعۡيَكُمۡ لَشَتّٰىؕ\u200F"},
{"verse_key":"92:5","id":6063,"text":"فَاَمَّا مَنۡ اَعۡطٰى وَاتَّقٰىۙ\u200F"},
{"verse_key":"92:6","id":6064,"text":"وَصَدَّقَ بِالۡحُسۡنٰىۙ\u200F"},
{"verse_key":"92:7","id":6065,"text":"فَسَنُيَسِّرُهٗ لِلۡيُسۡرٰىؕ\u200F"},
{"verse_key":"92:8","id":6066,"text":"وَاَمَّا مَنۡۢ بَخِلَ وَاسۡتَغۡنٰىۙ\u200F"},
{"verse_key":"92:9","id":6067,"text":"وَكَذَّبَ بِالۡحُسۡنٰىۙ\u200F"},
{"verse_key":"92:10","id":6068,"text":"فَسَنُيَسِّرُهٗ لِلۡعُسۡرٰىؕ\u200F"},
{"verse_key":"92:11","id":6069,"text":"وَمَا يُغۡنِىۡ عَنۡهُ مَالُهٗۤ اِذَا تَرَدّٰىؕ\u200F"},
{"verse_key":"92:12","id":6070,"text":"اِنَّ عَلَيۡنَا لَـلۡهُدٰىۖ\u200F"},
{"verse_key":"92:13","id":6071,"text":"وَاِنَّ لَـنَا لَـلۡاٰخِرَةَ وَالۡاُوۡلٰى\u200F"},
{"verse_key":"92:14","id":6072,"text":"فَاَنۡذَرۡتُكُمۡ نَارًا تَلَظّٰى\u200Bۚ\u200F"},
{"verse_key":"92:15","id":6073,"text":"لَا يَصۡلٰٮهَاۤ اِلَّا الۡاَشۡقَىۙ\u200F"},
{"verse_key":"92:16","id":6074,"text":"الَّذِىۡ كَذَّبَ وَتَوَلّٰىؕ\u200F"},
{"verse_key":"92:17","id":6075,"text":"وَسَيُجَنَّبُهَا الۡاَتۡقَىۙ\u200F"},
{"verse_key":"92:18","id":6076,"text":"الَّذِىۡ يُؤۡتِىۡ مَالَهٗ يَتَزَكّٰى\u200Bۚ\u200F"},
{"verse_key":"92:19","id":6077,"text":"وَمَا لِاَحَدٍ عِنۡدَهٗ مِنۡ نِّعۡمَةٍ تُجۡزٰٓىۙ\u200F"},
{"verse_key":"92:20","id":6078,"text":"اِلَّا ابۡتِغَآءَ وَجۡهِ رَبِّهِ الۡاَعۡلٰى\u200Bۚ\u200F"},
{"verse_key":"92:21","id":6079,"text":"وَلَسَوۡفَ يَرۡضٰى\u200F"}]}