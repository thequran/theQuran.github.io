{"surah":"77", "verses": [{"verse_key":"77:1","id":5623,"text":"وَالۡمُرۡسَلٰتِ عُرۡفًا ۙ\u200F"},
{"verse_key":"77:2","id":5624,"text":"فَالۡعٰصِفٰتِ عَصۡفًا ۙ\u200F"},
{"verse_key":"77:3","id":5625,"text":"وَّالنّٰشِرٰتِ نَشۡرًا ۙ\u200F"},
{"verse_key":"77:4","id":5626,"text":"فَالۡفٰرِقٰتِ فَرۡقًا ۙ\u200F"},
{"verse_key":"77:5","id":5627,"text":"فَالۡمُلۡقِيٰتِ ذِكۡرًا ۙ\u200F"},
{"verse_key":"77:6","id":5628,"text":"عُذۡرًا اَوۡ نُذۡرًا ۙ\u200F"},
{"verse_key":"77:7","id":5629,"text":"اِنَّمَا تُوۡعَدُوۡنَ لَوَاقِعٌ\u2002ؕ\u200F"},
{"verse_key":"77:8","id":5630,"text":"فَاِذَا النُّجُوۡمُ طُمِسَتۡۙ\u200F"},
{"verse_key":"77:9","id":5631,"text":"وَ اِذَا السَّمَآءُ فُرِجَتۡۙ\u200F"},
{"verse_key":"77:10","id":5632,"text":"وَاِذَا الۡجِبَالُ نُسِفَتۡۙ\u200F"},
{"verse_key":"77:11","id":5633,"text":"وَاِذَا الرُّسُلُ اُقِّتَتۡؕ\u200F"},
{"verse_key":"77:12","id":5634,"text":"لِاَىِّ يَوۡمٍ اُجِّلَتۡؕ\u200F"},
{"verse_key":"77:13","id":5635,"text":"لِيَوۡمِ الۡفَصۡلِ\u200Bۚ\u200F"},
{"verse_key":"77:14","id":5636,"text":"وَمَاۤ اَدۡرٰٮكَ مَا يَوۡمُ الۡفَصۡلِؕ\u200F"},
{"verse_key":"77:15","id":5637,"text":"وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ\u200F"},
{"verse_key":"77:16","id":5638,"text":"اَلَمۡ نُهۡلِكِ الۡاَوَّلِيۡنَؕ\u200F"},
{"verse_key":"77:17","id":5639,"text":"ثُمَّ نُتۡبِعُهُمُ الۡاٰخِرِيۡنَ\u200F"},
{"verse_key":"77:18","id":5640,"text":"كَذٰلِكَ نَفۡعَلُ بِالۡمُجۡرِمِيۡنَ\u200F"},
{"verse_key":"77:19","id":5641,"text":"وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ\u200F"},
{"verse_key":"77:20","id":5642,"text":"اَلَمۡ نَخۡلُقۡكُّمۡ مِّنۡ مَّآءٍ مَّهِيۡنٍۙ\u200F"},
{"verse_key":"77:21","id":5643,"text":"فَجَعَلۡنٰهُ فِىۡ قَرَارٍ مَّكِيۡنٍۙ\u200F"},
{"verse_key":"77:22","id":5644,"text":"اِلٰى قَدَرٍ مَّعۡلُوۡمٍۙ\u200F"},
{"verse_key":"77:23","id":5645,"text":"فَقَدَرۡنَا ۖ فَنِعۡمَ الۡقٰدِرُوۡنَ\u200F"},
{"verse_key":"77:24","id":5646,"text":"وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ\u200F"},
{"verse_key":"77:25","id":5647,"text":"اَلَمۡ نَجۡعَلِ الۡاَرۡضَ كِفَاتًا ۙ\u200F"},
{"verse_key":"77:26","id":5648,"text":"اَحۡيَآءً وَّاَمۡوَاتًا ۙ\u200F"},
{"verse_key":"77:27","id":5649,"text":"وَّجَعَلۡنَا فِيۡهَا رَوَاسِىَ شٰمِخٰتٍ وَّ اَسۡقَيۡنٰكُمۡ مَّآءً فُرَاتًا ؕ\u200F"},
{"verse_key":"77:28","id":5650,"text":"وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ\u200F"},
{"verse_key":"77:29","id":5651,"text":"اِنْطَلِقُوۡۤا اِلٰى مَا كُنۡتُمۡ بِهٖ تُكَذِّبُوۡنَ\u200Bۚ\u200F"},
{"verse_key":"77:30","id":5652,"text":"اِنْطَلِقُوۡۤا اِلٰى ظِلٍّ ذِىۡ ثَلٰثِ شُعَبٍۙ\u200F"},
{"verse_key":"77:31","id":5653,"text":"لَّا ظَلِيۡلٍ وَّلَا يُغۡنِىۡ مِنَ اللَّهَبِؕ\u200F"},
{"verse_key":"77:32","id":5654,"text":"اِنَّهَا تَرۡمِىۡ بِشَرَرٍ كَالۡقَصۡرِ\u200Bۚ\u200F"},
{"verse_key":"77:33","id":5655,"text":"كَاَنَّهٗ جِمٰلَتٌ صُفۡرٌ ؕ\u200F"},
{"verse_key":"77:34","id":5656,"text":"وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ\u200F"},
{"verse_key":"77:35","id":5657,"text":"هٰذَا يَوۡمُ لَا يَنۡطِقُوۡنَۙ\u200F"},
{"verse_key":"77:36","id":5658,"text":"وَلَا يُؤۡذَنُ لَهُمۡ فَيَـعۡتَذِرُوۡنَ\u200F"},
{"verse_key":"77:37","id":5659,"text":"وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ\u200F"},
{"verse_key":"77:38","id":5660,"text":"هٰذَا يَوۡمُ الۡفَصۡلِ\u200Bۚ جَمَعۡنٰكُمۡ وَالۡاَوَّلِيۡنَ\u200F"},
{"verse_key":"77:39","id":5661,"text":"فَاِنۡ كَانَ لَـكُمۡ كَيۡدٌ فَكِيۡدُوۡنِ\u200F"},
{"verse_key":"77:40","id":5662,"text":"وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ\u200F"},
{"verse_key":"77:41","id":5663,"text":"اِنَّ الۡمُتَّقِيۡنَ فِىۡ ظِلٰلٍ وَّعُيُوۡنٍۙ\u200F"},
{"verse_key":"77:42","id":5664,"text":"وَّفَوَاكِهَ مِمَّا يَشۡتَهُوۡنَؕ\u200F"},
{"verse_key":"77:43","id":5665,"text":"كُلُوۡا وَاشۡرَبُوۡا هَنِيْٓئًا ۢ بِمَا كُنۡتُمۡ تَعۡمَلُوۡنَ\u200F"},
{"verse_key":"77:44","id":5666,"text":"اِنَّا كَذٰلِكَ نَجۡزِى الۡمُحۡسِنِيۡنَ\u200F"},
{"verse_key":"77:45","id":5667,"text":"وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ\u200F"},
{"verse_key":"77:46","id":5668,"text":"كُلُوۡا وَتَمَتَّعُوۡا قَلِيۡلًا اِنَّكُمۡ مُّجۡرِمُوۡنَ\u200F"},
{"verse_key":"77:47","id":5669,"text":"وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ\u200F"},
{"verse_key":"77:48","id":5670,"text":"وَاِذَا قِيۡلَ لَهُمُ ارۡكَعُوۡا لَا يَرۡكَعُوۡنَ\u200F"},
{"verse_key":"77:49","id":5671,"text":"وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ\u200F"},
{"verse_key":"77:50","id":5672,"text":"فَبِاَىِّ حَدِيۡثٍۢ بَعۡدَهٗ يُؤۡمِنُوۡنَ\u200F"}]}