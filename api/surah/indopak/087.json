{"surah":"87", "verses": [{"verse_key":"87:1","id":5949,"text":"سَبِّحِ اسۡمَ رَبِّكَ الۡاَعۡلَىۙ\u200F"},
{"verse_key":"87:2","id":5950,"text":"الَّذِىۡ خَلَقَ فَسَوّٰى ۙ\u200F"},
{"verse_key":"87:3","id":5951,"text":"وَالَّذِىۡ قَدَّرَ فَهَدٰى ۙ\u200F"},
{"verse_key":"87:4","id":5952,"text":"وَالَّذِىۡۤ اَخۡرَجَ الۡمَرۡعٰى ۙ\u200F"},
{"verse_key":"87:5","id":5953,"text":"فَجَعَلَهٗ غُثَآءً اَحۡوٰىؕ\u200F"},
{"verse_key":"87:6","id":5954,"text":"سَنُقۡرِئُكَ فَلَا تَنۡسٰٓىۙ\u200F"},
{"verse_key":"87:7","id":5955,"text":"اِلَّا مَا شَآءَ اللّٰهُ\u200Bؕ اِنَّهٗ يَعۡلَمُ الۡجَهۡرَ وَمَا يَخۡفٰىؕ\u200F"},
{"verse_key":"87:8","id":5956,"text":"وَنُيَسِّرُكَ لِلۡيُسۡرٰى \u200Bۖ\u200Bۚ\u200F"},
{"verse_key":"87:9","id":5957,"text":"فَذَكِّرۡ اِنۡ نَّفَعَتِ الذِّكۡرٰىؕ\u200F"},
{"verse_key":"87:10","id":5958,"text":"سَيَذَّكَّرُ مَنۡ يَّخۡشٰىۙ\u200F"},
{"verse_key":"87:11","id":5959,"text":"وَيَتَجَنَّبُهَا الۡاَشۡقَىۙ\u200F"},
{"verse_key":"87:12","id":5960,"text":"الَّذِىۡ يَصۡلَى النَّارَ الۡكُبۡرٰى\u200Bۚ\u200F"},
{"verse_key":"87:13","id":5961,"text":"ثُمَّ لَا يَمُوۡتُ فِيۡهَا وَلَا يَحۡيٰىؕ\u200F"},
{"verse_key":"87:14","id":5962,"text":"قَدۡ اَفۡلَحَ مَنۡ تَزَكّٰىۙ\u200F"},
{"verse_key":"87:15","id":5963,"text":"وَذَكَرَ اسۡمَ رَبِّهٖ فَصَلّٰى\u200B ؕ\u200F"},
{"verse_key":"87:16","id":5964,"text":"بَلۡ تُؤۡثِرُوۡنَ الۡحَيٰوةَ الدُّنۡيَا\u2003ۖ\u200F"},
{"verse_key":"87:17","id":5965,"text":"وَالۡاٰخِرَةُ خَيۡرٌ وَّ اَبۡقٰىؕ\u200F"},
{"verse_key":"87:18","id":5966,"text":"اِنَّ هٰذَا لَفِى الصُّحُفِ الۡاُوۡلٰىۙ\u200F"},
{"verse_key":"87:19","id":5967,"text":"صُحُفِ اِبۡرٰهِيۡمَ وَمُوۡسٰى\u200F"}]}