{"surah":"75", "verses": [{"verse_key":"75:1","id":5552,"text":"لَاۤ اُقۡسِمُ بِيَوۡمِ الۡقِيٰمَةِۙ\u200F"},
{"verse_key":"75:2","id":5553,"text":"وَلَاۤ اُقۡسِمُ بِالنَّفۡسِ اللَّوَّامَةِؕ\u200F"},
{"verse_key":"75:3","id":5554,"text":"اَيَحۡسَبُ الۡاِنۡسَانُ اَلَّنۡ نَّجۡمَعَ عِظَامَهٗؕ\u200F"},
{"verse_key":"75:4","id":5555,"text":"بَلٰى قٰدِرِيۡنَ عَلٰٓى اَنۡ نُّسَوِّىَ بَنَانَهٗ\u200F"},
{"verse_key":"75:5","id":5556,"text":"بَلۡ يُرِيۡدُ الۡاِنۡسَانُ لِيَفۡجُرَ اَمَامَهٗ\u200Bۚ\u200F"},
{"verse_key":"75:6","id":5557,"text":"يَسۡـَٔـلُ اَيَّانَ يَوۡمُ الۡقِيٰمَةِؕ\u200F"},
{"verse_key":"75:7","id":5558,"text":"فَاِذَا بَرِقَ الۡبَصَرُۙ\u200F"},
{"verse_key":"75:8","id":5559,"text":"وَخَسَفَ الۡقَمَرُۙ\u200F"},
{"verse_key":"75:9","id":5560,"text":"وَجُمِعَ الشَّمۡسُ وَالۡقَمَرُۙ\u200F"},
{"verse_key":"75:10","id":5561,"text":"يَقُوۡلُ الۡاِنۡسَانُ يَوۡمَٮِٕذٍ اَيۡنَ الۡمَفَرُّ\u200B ۚ\u200F"},
{"verse_key":"75:11","id":5562,"text":"كَلَّا لَا وَزَرَؕ\u200F"},
{"verse_key":"75:12","id":5563,"text":"اِلٰى رَبِّكَ يَوۡمَٮِٕذِ اۨلۡمُسۡتَقَرُّ ؕ\u200F"},
{"verse_key":"75:13","id":5564,"text":"يُنَبَّؤُا الۡاِنۡسَانُ يَوۡمَٮِٕذٍۢ بِمَا قَدَّمَ وَاَخَّرَؕ\u200F"},
{"verse_key":"75:14","id":5565,"text":"بَلِ الۡاِنۡسَانُ عَلٰى نَفۡسِهٖ بَصِيۡرَةٌ\u2002ۙ\u200F"},
{"verse_key":"75:15","id":5566,"text":"وَّلَوۡ اَلۡقٰى مَعَاذِيۡرَهٗؕ\u200F"},
{"verse_key":"75:16","id":5567,"text":"لَا تُحَرِّكۡ بِهٖ لِسَانَكَ لِتَعۡجَلَ بِهٖؕ\u200F"},
{"verse_key":"75:17","id":5568,"text":"اِنَّ عَلَيۡنَا جَمۡعَهٗ وَقُرۡاٰنَهٗۚ \u2002ۖ\u200F"},
{"verse_key":"75:18","id":5569,"text":"فَاِذَا قَرَاۡنٰهُ فَاتَّبِعۡ قُرۡاٰنَهٗ\u200Bۚ\u200F"},
{"verse_key":"75:19","id":5570,"text":"ثُمَّ اِنَّ عَلَيۡنَا بَيَانَهٗؕ\u200F"},
{"verse_key":"75:20","id":5571,"text":"كَلَّا بَلۡ تُحِبُّوۡنَ الۡعَاجِلَةَ ۙ\u200F"},
{"verse_key":"75:21","id":5572,"text":"وَتَذَرُوۡنَ الۡاٰخِرَةَ ؕ\u200F"},
{"verse_key":"75:22","id":5573,"text":"وُجُوۡهٌ يَّوۡمَٮِٕذٍ نَّاضِرَةٌ\u2002ۙ\u200F"},
{"verse_key":"75:23","id":5574,"text":"اِلٰى رَبِّهَا نَاظِرَةٌ\u200B\u2002ۚ\u200F"},
{"verse_key":"75:24","id":5575,"text":"وَوُجُوۡهٌ يَّوۡمَٮِٕذٍۢ بَاسِرَةٌ\u2002ۙ\u200F"},
{"verse_key":"75:25","id":5576,"text":"تَظُنُّ اَنۡ يُّفۡعَلَ بِهَا فَاقِرَةٌ\u2002ؕ\u200F"},
{"verse_key":"75:26","id":5577,"text":"كَلَّاۤ اِذَا بَلَغَتِ التَّرَاقِىَۙ\u200F"},
{"verse_key":"75:27","id":5578,"text":"وَقِيۡلَ مَنۡ رَاقٍۙ\u200F"},
{"verse_key":"75:28","id":5579,"text":"وَّظَنَّ اَنَّهُ الۡفِرَاقُۙ\u200F"},
{"verse_key":"75:29","id":5580,"text":"وَالۡتَفَّتِ السَّاقُ بِالسَّاقِۙ\u200F"},
{"verse_key":"75:30","id":5581,"text":"اِلٰى رَبِّكَ يَوۡمَٮِٕذِ اۨلۡمَسَاقُؕ\u200F"},
{"verse_key":"75:31","id":5582,"text":"فَلَا صَدَّقَ وَلَا صَلّٰىۙ\u200F"},
{"verse_key":"75:32","id":5583,"text":"وَلٰڪِنۡ كَذَّبَ وَتَوَلّٰىۙ\u200F"},
{"verse_key":"75:33","id":5584,"text":"ثُمَّ ذَهَبَ اِلٰٓى اَهۡلِهٖ يَتَمَطّٰىؕ\u200F"},
{"verse_key":"75:34","id":5585,"text":"اَوۡلٰى لَكَ فَاَوۡلٰىۙ\u200F"},
{"verse_key":"75:35","id":5586,"text":"ثُمَّ اَوۡلٰى لَكَ فَاَوۡلٰىؕ\u200F"},
{"verse_key":"75:36","id":5587,"text":"اَيَحۡسَبُ الۡاِنۡسَانُ اَنۡ يُّتۡرَكَ سُدًىؕ\u200F"},
{"verse_key":"75:37","id":5588,"text":"اَلَمۡ يَكُ نُطۡفَةً مِّنۡ مَّنِىٍّ يُّمۡنٰىۙ\u200F"},
{"verse_key":"75:38","id":5589,"text":"ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوّٰىۙ\u200F"},
{"verse_key":"75:39","id":5590,"text":"فَجَعَلَ مِنۡهُ الزَّوۡجَيۡنِ الذَّكَرَ وَالۡاُنۡثٰىؕ\u200F"},
{"verse_key":"75:40","id":5591,"text":"اَلَيۡسَ ذٰلِكَ بِقٰدِرٍ عَلٰٓى اَنۡ يُّحۡـىِۦَ الۡمَوۡتٰى\u200F"}]}