{"surah":"80", "verses": [{"verse_key":"80:1","id":5759,"text":"عَبَسَ وَتَوَلّٰٓىۙ\u200F"},
{"verse_key":"80:2","id":5760,"text":"اَنۡ جَآءَهُ الۡاَعۡمٰىؕ\u200F"},
{"verse_key":"80:3","id":5761,"text":"وَمَا يُدۡرِيۡكَ لَعَلَّهٗ يَزَّكّٰٓىۙ\u200F"},
{"verse_key":"80:4","id":5762,"text":"اَوۡ يَذَّكَّرُ فَتَنۡفَعَهُ الذِّكۡرٰىؕ\u200F"},
{"verse_key":"80:5","id":5763,"text":"اَمَّا مَنِ اسۡتَغۡنٰىۙ\u200F"},
{"verse_key":"80:6","id":5764,"text":"فَاَنۡتَ لَهٗ تَصَدّٰىؕ\u200F"},
{"verse_key":"80:7","id":5765,"text":"وَمَا عَلَيۡكَ اَلَّا يَزَّكّٰٓىؕ\u200F"},
{"verse_key":"80:8","id":5766,"text":"وَاَمَّا مَنۡ جَآءَكَ يَسۡعٰىۙ\u200F"},
{"verse_key":"80:9","id":5767,"text":"وَهُوَ يَخۡشٰىۙ\u200F"},
{"verse_key":"80:10","id":5768,"text":"فَاَنۡتَ عَنۡهُ تَلَهّٰى\u200Bۚ\u200F"},
{"verse_key":"80:11","id":5769,"text":"كَلَّاۤ اِنَّهَا تَذۡكِرَةٌ\u2002ۚ\u200F"},
{"verse_key":"80:12","id":5770,"text":"فَمَنۡ شَآءَ ذَكَرَهٗ\u200Bۘ\u200F"},
{"verse_key":"80:13","id":5771,"text":"فِىۡ صُحُفٍ مُّكَرَّمَةٍۙ\u200F"},
{"verse_key":"80:14","id":5772,"text":"مَّرۡفُوۡعَةٍ مُّطَهَّرَةٍ ۭۙ\u200F"},
{"verse_key":"80:15","id":5773,"text":"بِاَيۡدِىۡ سَفَرَةٍۙ\u200F"},
{"verse_key":"80:16","id":5774,"text":"كِرَامٍۢ بَرَرَةٍؕ\u200F"},
{"verse_key":"80:17","id":5775,"text":"قُتِلَ الۡاِنۡسَانُ مَاۤ اَكۡفَرَهٗؕ\u200F"},
{"verse_key":"80:18","id":5776,"text":"مِنۡ اَىِّ شَىۡءٍ خَلَقَهٗؕ\u200F"},
{"verse_key":"80:19","id":5777,"text":"مِنۡ نُّطۡفَةٍؕ خَلَقَهٗ فَقَدَّرَهٗ ۙ\u200F"},
{"verse_key":"80:20","id":5778,"text":"ثُمَّ السَّبِيۡلَ يَسَّرَهٗۙ\u200F"},
{"verse_key":"80:21","id":5779,"text":"ثُمَّ اَمَاتَهٗ فَاَقۡبَرَهٗۙ\u200F"},
{"verse_key":"80:22","id":5780,"text":"ثُمَّ اِذَا شَآءَ اَنۡشَرَهٗؕ\u200F"},
{"verse_key":"80:23","id":5781,"text":"كَلَّا لَـمَّا يَقۡضِ مَاۤ اَمَرَهٗؕ\u200F"},
{"verse_key":"80:24","id":5782,"text":"فَلۡيَنۡظُرِ الۡاِنۡسَانُ اِلٰى طَعَامِهٖۤۙ\u200F"},
{"verse_key":"80:25","id":5783,"text":"اَنَّا صَبَبۡنَا الۡمَآءَ صَبًّا ۙ\u200F"},
{"verse_key":"80:26","id":5784,"text":"ثُمَّ شَقَقۡنَا الۡاَرۡضَ شَقًّا ۙ\u200F"},
{"verse_key":"80:27","id":5785,"text":"فَاَنۡۢبَتۡنَا فِيۡهَا حَبًّا ۙ\u200F"},
{"verse_key":"80:28","id":5786,"text":"وَّ عِنَبًا وَّقَضۡبًا ۙ\u200F"},
{"verse_key":"80:29","id":5787,"text":"وَّزَيۡتُوۡنًا وَّنَخۡلًا ؕ\u200F"},
{"verse_key":"80:30","id":5788,"text":"وَحَدَآٮِٕقَ غُلۡبًا ۙ\u200F"},
{"verse_key":"80:31","id":5789,"text":"وَّفَاكِهَةً وَّاَبًّا ۙ\u200F"},
{"verse_key":"80:32","id":5790,"text":"مَّتَاعًا لَّـكُمۡ وَلِاَنۡعَامِكُمۡؕ\u200F"},
{"verse_key":"80:33","id":5791,"text":"فَاِذَا جَآءَتِ الصَّآخَّةُ\u200F"},
{"verse_key":"80:34","id":5792,"text":"يَوۡمَ يَفِرُّ الۡمَرۡءُ مِنۡ اَخِيۡهِۙ\u200F"},
{"verse_key":"80:35","id":5793,"text":"وَاُمِّهٖ وَاَبِيۡهِۙ\u200F"},
{"verse_key":"80:36","id":5794,"text":"وَصَاحِبَتِهٖ وَبَنِيۡهِؕ\u200F"},
{"verse_key":"80:37","id":5795,"text":"لِكُلِّ امۡرِیءٍ مِّنۡهُمۡ يَوۡمَٮِٕذٍ شَاۡنٌ يُّغۡنِيۡهِؕ\u200F"},
{"verse_key":"80:38","id":5796,"text":"وُجُوۡهٌ يَّوۡمَٮِٕذٍ مُّسۡفِرَةٌ\u2002ۙ\u200F"},
{"verse_key":"80:39","id":5797,"text":"ضَاحِكَةٌ مُّسۡتَبۡشِرَةٌ\u2002ۚ\u200F"},
{"verse_key":"80:40","id":5798,"text":"وَوُجُوۡهٌ يَّوۡمَٮِٕذٍ عَلَيۡهَا غَبَرَةٌ\u2002ۙ\u200F"},
{"verse_key":"80:41","id":5799,"text":"تَرۡهَقُهَا قَتَرَةٌ\u2002ؕ\u200F"},
{"verse_key":"80:42","id":5800,"text":"اُولٰٓٮِٕكَ هُمُ الۡكَفَرَةُ الۡفَجَرَةُ\u200F"}]}