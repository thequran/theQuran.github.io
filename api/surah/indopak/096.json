{"surah":"96", "verses": [{"verse_key":"96:1","id":6107,"text":"اِقۡرَاۡ بِاسۡمِ رَبِّكَ الَّذِىۡ خَلَقَ\u200Bۚ\u200F"},
{"verse_key":"96:2","id":6108,"text":"خَلَقَ الۡاِنۡسَانَ مِنۡ عَلَقٍ\u200Bۚ\u200F"},
{"verse_key":"96:3","id":6109,"text":"اِقۡرَاۡ وَرَبُّكَ الۡاَكۡرَمُۙ\u200F"},
{"verse_key":"96:4","id":6110,"text":"الَّذِىۡ عَلَّمَ بِالۡقَلَمِۙ\u200F"},
{"verse_key":"96:5","id":6111,"text":"عَلَّمَ الۡاِنۡسَانَ مَا لَمۡ يَعۡلَمۡؕ\u200F"},
{"verse_key":"96:6","id":6112,"text":"كَلَّاۤ اِنَّ الۡاِنۡسَانَ لَيَطۡغٰٓىۙ\u200F"},
{"verse_key":"96:7","id":6113,"text":"اَنۡ رَّاٰهُ اسۡتَغۡنٰىؕ\u200F"},
{"verse_key":"96:8","id":6114,"text":"اِنَّ اِلٰى رَبِّكَ الرُّجۡعٰىؕ\u200F"},
{"verse_key":"96:9","id":6115,"text":"اَرَءَيۡتَ الَّذِىۡ يَنۡهٰىؕ\u200F"},
{"verse_key":"96:10","id":6116,"text":"عَبۡدًا اِذَا صَلّٰىؕ\u200F"},
{"verse_key":"96:11","id":6117,"text":"اَرَءَيۡتَ اِنۡ كَانَ عَلَى الۡهُدٰٓىۙ\u200F"},
{"verse_key":"96:12","id":6118,"text":"اَوۡ اَمَرَ بِالتَّقۡوٰىۙ\u200F"},
{"verse_key":"96:13","id":6119,"text":"اَرَءَيۡتَ اِنۡ كَذَّبَ وَتَوَلّٰىؕ\u200F"},
{"verse_key":"96:14","id":6120,"text":"اَلَمۡ يَعۡلَمۡ بِاَنَّ اللّٰهَ يَرٰىؕ\u200F"},
{"verse_key":"96:15","id":6121,"text":"كَلَّا لَٮِٕنۡ لَّمۡ يَنۡتَهِ\u2003 ۙ لَنَسۡفَعًۢا بِالنَّاصِيَةِۙ\u200F"},
{"verse_key":"96:16","id":6122,"text":"نَاصِيَةٍ كَاذِبَةٍ خَاطِئَةٍ\u200B ۚ\u200F"},
{"verse_key":"96:17","id":6123,"text":"فَلۡيَدۡعُ نَادِيَهٗ ۙ\u200F"},
{"verse_key":"96:18","id":6124,"text":"سَنَدۡعُ الزَّبَانِيَةَ ۙ\u200F"},
{"verse_key":"96:19","id":6125,"text":"كَلَّا ؕ لَا تُطِعۡهُ وَاسۡجُدۡ وَاقۡتَرِبْ۩\u200F"}]}