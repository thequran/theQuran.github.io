{"surah":"74", "verses": [{"verse_key":"74:1","id":5496,"text":"يٰۤاَيُّهَا الۡمُدَّثِّرُۙ\u200F"},
{"verse_key":"74:2","id":5497,"text":"قُمۡ فَاَنۡذِرۡۙ\u200F"},
{"verse_key":"74:3","id":5498,"text":"وَرَبَّكَ فَكَبِّرۡۙ\u200F"},
{"verse_key":"74:4","id":5499,"text":"وَثِيَابَكَ فَطَهِّرۡۙ\u200F"},
{"verse_key":"74:5","id":5500,"text":"وَالرُّجۡزَ فَاهۡجُرۡۙ\u200F"},
{"verse_key":"74:6","id":5501,"text":"وَلَا تَمۡنُنۡ تَسۡتَكۡثِرُۙ\u200F"},
{"verse_key":"74:7","id":5502,"text":"وَ لِرَبِّكَ فَاصۡبِرۡؕ\u200F"},
{"verse_key":"74:8","id":5503,"text":"فَاِذَا نُقِرَ فِى النَّاقُوۡرِۙ\u200F"},
{"verse_key":"74:9","id":5504,"text":"فَذٰلِكَ يَوۡمَٮِٕذٍ يَّوۡمٌ عَسِيۡرٌۙ\u200F"},
{"verse_key":"74:10","id":5505,"text":"عَلَى الۡكٰفِرِيۡنَ غَيۡرُ يَسِيۡرٍ\u200F"},
{"verse_key":"74:11","id":5506,"text":"ذَرۡنِىۡ وَمَنۡ خَلَقۡتُ وَحِيۡدًا ۙ\u200F"},
{"verse_key":"74:12","id":5507,"text":"وَّجَعَلۡتُ لَهٗ مَالًا مَّمۡدُوۡدًا ۙ\u200F"},
{"verse_key":"74:13","id":5508,"text":"وَّبَنِيۡنَ شُهُوۡدًا ۙ\u200F"},
{"verse_key":"74:14","id":5509,"text":"وَّمَهَّدتُّ لَهٗ تَمۡهِيۡدًا ۙ\u200F"},
{"verse_key":"74:15","id":5510,"text":"ثُمَّ يَطۡمَعُ اَنۡ اَزِيۡدَ \u2002ۙ\u200F"},
{"verse_key":"74:16","id":5511,"text":"كَلَّا ؕ اِنَّهٗ كَانَ لِاٰيٰتِنَا عَنِيۡدًا ؕ\u200F"},
{"verse_key":"74:17","id":5512,"text":"سَاُرۡهِقُهٗ صَعُوۡدًا ؕ\u200F"},
{"verse_key":"74:18","id":5513,"text":"اِنَّهٗ فَكَّرَ وَقَدَّرَۙ\u200F"},
{"verse_key":"74:19","id":5514,"text":"فَقُتِلَ كَيۡفَ قَدَّرَۙ\u200F"},
{"verse_key":"74:20","id":5515,"text":"ثُمَّ قُتِلَ كَيۡفَ قَدَّرَۙ\u200F"},
{"verse_key":"74:21","id":5516,"text":"ثُمَّ نَظَرَۙ\u200F"},
{"verse_key":"74:22","id":5517,"text":"ثُمَّ عَبَسَ وَبَسَرَۙ\u200F"},
{"verse_key":"74:23","id":5518,"text":"ثُمَّ اَدۡبَرَ وَاسۡتَكۡبَرَۙ\u200F"},
{"verse_key":"74:24","id":5519,"text":"فَقَالَ اِنۡ هٰذَاۤ اِلَّا سِحۡرٌ يُّؤۡثَرُۙ\u200F"},
{"verse_key":"74:25","id":5520,"text":"اِنۡ هٰذَاۤ اِلَّا قَوۡلُ الۡبَشَرِؕ\u200F"},
{"verse_key":"74:26","id":5521,"text":"سَاُصۡلِيۡهِ سَقَرَ\u200F"},
{"verse_key":"74:27","id":5522,"text":"وَمَاۤ اَدۡرٰٮكَ مَا سَقَرُؕ\u200F"},
{"verse_key":"74:28","id":5523,"text":"لَا تُبۡقِىۡ وَ لَا تَذَرُ\u200Bۚ\u200F"},
{"verse_key":"74:29","id":5524,"text":"لَـوَّاحَةٌ لِّلۡبَشَرِ\u200Bۖ\u200Bۚ\u200F"},
{"verse_key":"74:30","id":5525,"text":"عَلَيۡهَا تِسۡعَةَ عَشَرَؕ\u200F"},
{"verse_key":"74:31","id":5526,"text":"وَمَا جَعَلۡنَاۤ اَصۡحٰبَ النَّارِ اِلَّا مَلٰٓٮِٕكَةً\u200B وَّمَا جَعَلۡنَا عِدَّتَهُمۡ اِلَّا فِتۡنَةً لِّلَّذِيۡنَ كَفَرُوۡا ۙ لِيَسۡتَيۡقِنَ الَّذِيۡنَ اُوۡتُوا الۡكِتٰبَ وَيَزۡدَادَ الَّذِيۡنَ اٰمَنُوۡۤا اِيۡمَانًا\u200B وَّلَا يَرۡتَابَ الَّذِيۡنَ اُوۡتُوا الۡكِتٰبَ وَالۡمُؤۡمِنُوۡنَ\u200Bۙ وَلِيَقُوۡلَ الَّذِيۡنَ فِىۡ قُلُوۡبِهِمۡ مَّرَضٌ وَّالۡكٰفِرُوۡنَ مَاذَاۤ اَرَادَ اللّٰهُ بِهٰذَا مَثَلًا \u200Bؕ كَذٰلِكَ يُضِلُّ اللّٰهُ مَنۡ يَّشَآءُ وَيَهۡدِىۡ مَنۡ يَّشَآءُ \u200Bؕ وَمَا يَعۡلَمُ جُنُوۡدَ رَبِّكَ اِلَّا هُوَ \u200Bؕ وَمَا هِىَ اِلَّا ذِكۡرٰى لِلۡبَشَرِ\u200F"},
{"verse_key":"74:32","id":5527,"text":"كَلَّا وَالۡقَمَرِۙ\u200F"},
{"verse_key":"74:33","id":5528,"text":"وَالَّيۡلِ اِذۡ اَدۡبَرَۙ\u200F"},
{"verse_key":"74:34","id":5529,"text":"وَالصُّبۡحِ اِذَاۤ اَسۡفَرَۙ\u200F"},
{"verse_key":"74:35","id":5530,"text":"اِنَّهَا لَاِحۡدَى الۡكُبَرِۙ\u200F"},
{"verse_key":"74:36","id":5531,"text":"نَذِيۡرًا لِّلۡبَشَرِۙ\u200F"},
{"verse_key":"74:37","id":5532,"text":"لِمَنۡ شَآءَ مِنۡكُمۡ اَنۡ يَّتَقَدَّمَ اَوۡ يَتَاَخَّرَؕ\u200F"},
{"verse_key":"74:38","id":5533,"text":"كُلُّ نَفۡسٍ ۢ بِمَا كَسَبَتۡ رَهِيۡنَةٌ ۙ\u200F"},
{"verse_key":"74:39","id":5534,"text":"اِلَّاۤ اَصۡحٰبَ الۡيَمِيۡنِۛ ؕ\u200F"},
{"verse_key":"74:40","id":5535,"text":"فِىۡ جَنّٰتٍ ۛ يَتَسَآءَلُوۡنَۙ\u200F"},
{"verse_key":"74:41","id":5536,"text":"عَنِ الۡمُجۡرِمِيۡنَۙ\u200F"},
{"verse_key":"74:42","id":5537,"text":"مَا سَلَـكَكُمۡ فِىۡ سَقَرَ\u200F"},
{"verse_key":"74:43","id":5538,"text":"قَالُوۡا لَمۡ نَكُ مِنَ الۡمُصَلِّيۡنَۙ\u200F"},
{"verse_key":"74:44","id":5539,"text":"وَلَمۡ نَكُ نُطۡعِمُ الۡمِسۡكِيۡنَۙ\u200F"},
{"verse_key":"74:45","id":5540,"text":"وَكُنَّا نَخُوۡضُ مَعَ الۡخَـآٮِٕضِيۡنَۙ\u200F"},
{"verse_key":"74:46","id":5541,"text":"وَ كُنَّا نُكَذِّبُ بِيَوۡمِ الدِّيۡنِۙ\u200F"},
{"verse_key":"74:47","id":5542,"text":"حَتّٰٓى اَتٰٮنَا الۡيَقِيۡنُؕ\u200F"},
{"verse_key":"74:48","id":5543,"text":"فَمَا تَنۡفَعُهُمۡ شَفَاعَةُ الشّٰفِعِيۡنَؕ\u200F"},
{"verse_key":"74:49","id":5544,"text":"فَمَا لَهُمۡ عَنِ التَّذۡكِرَةِ مُعۡرِضِيۡنَۙ\u200F"},
{"verse_key":"74:50","id":5545,"text":"كَاَنَّهُمۡ حُمُرٌ مُّسۡتَنۡفِرَةٌ\u2002ۙ\u200F"},
{"verse_key":"74:51","id":5546,"text":"فَرَّتۡ مِنۡ قَسۡوَرَةٍ ؕ\u200F"},
{"verse_key":"74:52","id":5547,"text":"بَلۡ يُرِيۡدُ كُلُّ امۡرِىٴٍ مِّنۡهُمۡ اَنۡ يُّؤۡتٰى صُحُفًا مُّنَشَّرَةً\u2002ۙ\u200F ﻿﻿"},
{"verse_key":"74:53","id":5548,"text":"كَلَّا \u200Bؕ بَلۡ لَّا يَخَافُوۡنَ الۡاٰخِرَةَ ؕ\u200F"},
{"verse_key":"74:54","id":5549,"text":"كَلَّاۤ اِنَّهٗ تَذۡكِرَةٌ\u200B ۚ\u200F"},
{"verse_key":"74:55","id":5550,"text":"فَمَنۡ شَآءَ ذَكَرَهٗ ؕ\u200F"},
{"verse_key":"74:56","id":5551,"text":"وَمَا يَذۡكُرُوۡنَ اِلَّاۤ اَنۡ يَّشَآءَ اللّٰهُ\u200B ؕ هُوَ اَهۡلُ التَّقۡوٰى وَاَهۡلُ الۡمَغۡفِرَةِ\u200F"}]}