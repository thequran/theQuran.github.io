{"surah":"77", "verses": [{"verse_key":"77:1","id":5623,"text":" وَٱلْمُرْسَلَـٰتِ عُرْفًا"},
{"verse_key":"77:2","id":5624,"text":"فَٱلْعَـٰصِفَـٰتِ عَصْفًا"},
{"verse_key":"77:3","id":5625,"text":"وَٱلنَّـٰشِرَٰتِ نَشْرًا"},
{"verse_key":"77:4","id":5626,"text":"فَٱلْفَـٰرِقَـٰتِ فَرْقًا"},
{"verse_key":"77:5","id":5627,"text":"فَٱلْمُلْقِيَـٰتِ ذِكْرًا"},
{"verse_key":"77:6","id":5628,"text":"عُذْرًا أَوْ نُذْرًا"},
{"verse_key":"77:7","id":5629,"text":"إِنَّمَا تُوعَدُونَ لَوَٰقِعٌ"},
{"verse_key":"77:8","id":5630,"text":"فَإِذَا ٱلنُّجُومُ طُمِسَتْ"},
{"verse_key":"77:9","id":5631,"text":"وَإِذَا ٱلسَّمَآءُ فُرِجَتْ"},
{"verse_key":"77:10","id":5632,"text":"وَإِذَا ٱلْجِبَالُ نُسِفَتْ"},
{"verse_key":"77:11","id":5633,"text":"وَإِذَا ٱلرُّسُلُ أُقِّتَتْ"},
{"verse_key":"77:12","id":5634,"text":"لِأَىِّ يَوْمٍ أُجِّلَتْ"},
{"verse_key":"77:13","id":5635,"text":"لِيَوْمِ ٱلْفَصْلِ"},
{"verse_key":"77:14","id":5636,"text":"وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلْفَصْلِ"},
{"verse_key":"77:15","id":5637,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:16","id":5638,"text":"أَلَمْ نُهْلِكِ ٱلْأَوَّلِينَ"},
{"verse_key":"77:17","id":5639,"text":"ثُمَّ نُتْبِعُهُمُ ٱلْـَٔاخِرِينَ"},
{"verse_key":"77:18","id":5640,"text":"كَذَٰلِكَ نَفْعَلُ بِٱلْمُجْرِمِينَ"},
{"verse_key":"77:19","id":5641,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:20","id":5642,"text":"أَلَمْ نَخْلُقكُّم مِّن مَّآءٍ مَّهِينٍ"},
{"verse_key":"77:21","id":5643,"text":"فَجَعَلْنَـٰهُ فِى قَرَارٍ مَّكِينٍ"},
{"verse_key":"77:22","id":5644,"text":"إِلَىٰ قَدَرٍ مَّعْلُومٍ"},
{"verse_key":"77:23","id":5645,"text":"فَقَدَرْنَا فَنِعْمَ ٱلْقَـٰدِرُونَ"},
{"verse_key":"77:24","id":5646,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:25","id":5647,"text":"أَلَمْ نَجْعَلِ ٱلْأَرْضَ كِفَاتًا"},
{"verse_key":"77:26","id":5648,"text":"أَحْيَآءً وَأَمْوَٰتًا"},
{"verse_key":"77:27","id":5649,"text":"وَجَعَلْنَا فِيهَا رَوَٰسِىَ شَـٰمِخَـٰتٍ وَأَسْقَيْنَـٰكُم مَّآءً فُرَاتًا"},
{"verse_key":"77:28","id":5650,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:29","id":5651,"text":"ٱنطَلِقُوٓا۟ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ"},
{"verse_key":"77:30","id":5652,"text":"ٱنطَلِقُوٓا۟ إِلَىٰ ظِلٍّ ذِى ثَلَـٰثِ شُعَبٍ"},
{"verse_key":"77:31","id":5653,"text":"لَّا ظَلِيلٍ وَلَا يُغْنِى مِنَ ٱللَّهَبِ"},
{"verse_key":"77:32","id":5654,"text":"إِنَّهَا تَرْمِى بِشَرَرٍ كَٱلْقَصْرِ"},
{"verse_key":"77:33","id":5655,"text":"كَأَنَّهُۥ جِمَـٰلَتٌ صُفْرٌ"},
{"verse_key":"77:34","id":5656,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:35","id":5657,"text":"هَـٰذَا يَوْمُ لَا يَنطِقُونَ"},
{"verse_key":"77:36","id":5658,"text":"وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ"},
{"verse_key":"77:37","id":5659,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:38","id":5660,"text":"هَـٰذَا يَوْمُ ٱلْفَصْلِ ۖ جَمَعْنَـٰكُمْ وَٱلْأَوَّلِينَ"},
{"verse_key":"77:39","id":5661,"text":"فَإِن كَانَ لَكُمْ كَيْدٌ فَكِيدُونِ"},
{"verse_key":"77:40","id":5662,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:41","id":5663,"text":"إِنَّ ٱلْمُتَّقِينَ فِى ظِلَـٰلٍ وَعُيُونٍ"},
{"verse_key":"77:42","id":5664,"text":"وَفَوَٰكِهَ مِمَّا يَشْتَهُونَ"},
{"verse_key":"77:43","id":5665,"text":"كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَا كُنتُمْ تَعْمَلُونَ"},
{"verse_key":"77:44","id":5666,"text":"إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ"},
{"verse_key":"77:45","id":5667,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:46","id":5668,"text":"كُلُوا۟ وَتَمَتَّعُوا۟ قَلِيلًا إِنَّكُم مُّجْرِمُونَ"},
{"verse_key":"77:47","id":5669,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:48","id":5670,"text":"وَإِذَا قِيلَ لَهُمُ ٱرْكَعُوا۟ لَا يَرْكَعُونَ"},
{"verse_key":"77:49","id":5671,"text":"وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"},
{"verse_key":"77:50","id":5672,"text":"فَبِأَىِّ حَدِيثٍۭ بَعْدَهُۥ يُؤْمِنُونَ"}]}