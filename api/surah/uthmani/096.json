{"surah":"96", "verses": [{"verse_key":"96:1","id":6107,"text":" ٱقْرَأْ بِٱسْمِ رَبِّكَ ٱلَّذِى خَلَقَ"},
{"verse_key":"96:2","id":6108,"text":"خَلَقَ ٱلْإِنسَـٰنَ مِنْ عَلَقٍ"},
{"verse_key":"96:3","id":6109,"text":"ٱقْرَأْ وَرَبُّكَ ٱلْأَكْرَمُ"},
{"verse_key":"96:4","id":6110,"text":"ٱلَّذِى عَلَّمَ بِٱلْقَلَمِ"},
{"verse_key":"96:5","id":6111,"text":"عَلَّمَ ٱلْإِنسَـٰنَ مَا لَمْ يَعْلَمْ"},
{"verse_key":"96:6","id":6112,"text":"كَلَّآ إِنَّ ٱلْإِنسَـٰنَ لَيَطْغَىٰٓ"},
{"verse_key":"96:7","id":6113,"text":"أَن رَّءَاهُ ٱسْتَغْنَىٰٓ"},
{"verse_key":"96:8","id":6114,"text":"إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجْعَىٰٓ"},
{"verse_key":"96:9","id":6115,"text":"أَرَءَيْتَ ٱلَّذِى يَنْهَىٰ"},
{"verse_key":"96:10","id":6116,"text":"عَبْدًا إِذَا صَلَّىٰٓ"},
{"verse_key":"96:11","id":6117,"text":"أَرَءَيْتَ إِن كَانَ عَلَى ٱلْهُدَىٰٓ"},
{"verse_key":"96:12","id":6118,"text":"أَوْ أَمَرَ بِٱلتَّقْوَىٰٓ"},
{"verse_key":"96:13","id":6119,"text":"أَرَءَيْتَ إِن كَذَّبَ وَتَوَلَّىٰٓ"},
{"verse_key":"96:14","id":6120,"text":"أَلَمْ يَعْلَم بِأَنَّ ٱللَّهَ يَرَىٰ"},
{"verse_key":"96:15","id":6121,"text":"كَلَّا لَئِن لَّمْ يَنتَهِ لَنَسْفَعًۢا بِٱلنَّاصِيَةِ"},
{"verse_key":"96:16","id":6122,"text":"نَاصِيَةٍ كَـٰذِبَةٍ خَاطِئَةٍ"},
{"verse_key":"96:17","id":6123,"text":"فَلْيَدْعُ نَادِيَهُۥ"},
{"verse_key":"96:18","id":6124,"text":"سَنَدْعُ ٱلزَّبَانِيَةَ"},
{"verse_key":"96:19","id":6125,"text":"كَلَّا لَا تُطِعْهُ وَٱسْجُدْ وَٱقْتَرِب ۩"}]}