{"surah":"81", "verses": [{"verse_key":"81:1","id":5801,"text":" إِذَا ٱلشَّمْسُ كُوِّرَتْ"},
{"verse_key":"81:2","id":5802,"text":"وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتْ"},
{"verse_key":"81:3","id":5803,"text":"وَإِذَا ٱلْجِبَالُ سُيِّرَتْ"},
{"verse_key":"81:4","id":5804,"text":"وَإِذَا ٱلْعِشَارُ عُطِّلَتْ"},
{"verse_key":"81:5","id":5805,"text":"وَإِذَا ٱلْوُحُوشُ حُشِرَتْ"},
{"verse_key":"81:6","id":5806,"text":"وَإِذَا ٱلْبِحَارُ سُجِّرَتْ"},
{"verse_key":"81:7","id":5807,"text":"وَإِذَا ٱلنُّفُوسُ زُوِّجَتْ"},
{"verse_key":"81:8","id":5808,"text":"وَإِذَا ٱلْمَوْءُۥدَةُ سُئِلَتْ"},
{"verse_key":"81:9","id":5809,"text":"بِأَىِّ ذَنۢبٍ قُتِلَتْ"},
{"verse_key":"81:10","id":5810,"text":"وَإِذَا ٱلصُّحُفُ نُشِرَتْ"},
{"verse_key":"81:11","id":5811,"text":"وَإِذَا ٱلسَّمَآءُ كُشِطَتْ"},
{"verse_key":"81:12","id":5812,"text":"وَإِذَا ٱلْجَحِيمُ سُعِّرَتْ"},
{"verse_key":"81:13","id":5813,"text":"وَإِذَا ٱلْجَنَّةُ أُزْلِفَتْ"},
{"verse_key":"81:14","id":5814,"text":"عَلِمَتْ نَفْسٌ مَّآ أَحْضَرَتْ"},
{"verse_key":"81:15","id":5815,"text":"فَلَآ أُقْسِمُ بِٱلْخُنَّسِ"},
{"verse_key":"81:16","id":5816,"text":"ٱلْجَوَارِ ٱلْكُنَّسِ"},
{"verse_key":"81:17","id":5817,"text":"وَٱلَّيْلِ إِذَا عَسْعَسَ"},
{"verse_key":"81:18","id":5818,"text":"وَٱلصُّبْحِ إِذَا تَنَفَّسَ"},
{"verse_key":"81:19","id":5819,"text":"إِنَّهُۥ لَقَوْلُ رَسُولٍ كَرِيمٍ"},
{"verse_key":"81:20","id":5820,"text":"ذِى قُوَّةٍ عِندَ ذِى ٱلْعَرْشِ مَكِينٍ"},
{"verse_key":"81:21","id":5821,"text":"مُّطَاعٍ ثَمَّ أَمِينٍ"},
{"verse_key":"81:22","id":5822,"text":"وَمَا صَاحِبُكُم بِمَجْنُونٍ"},
{"verse_key":"81:23","id":5823,"text":"وَلَقَدْ رَءَاهُ بِٱلْأُفُقِ ٱلْمُبِينِ"},
{"verse_key":"81:24","id":5824,"text":"وَمَا هُوَ عَلَى ٱلْغَيْبِ بِضَنِينٍ"},
{"verse_key":"81:25","id":5825,"text":"وَمَا هُوَ بِقَوْلِ شَيْطَـٰنٍ رَّجِيمٍ"},
{"verse_key":"81:26","id":5826,"text":"فَأَيْنَ تَذْهَبُونَ"},
{"verse_key":"81:27","id":5827,"text":"إِنْ هُوَ إِلَّا ذِكْرٌ لِّلْعَـٰلَمِينَ"},
{"verse_key":"81:28","id":5828,"text":"لِمَن شَآءَ مِنكُمْ أَن يَسْتَقِيمَ"},
{"verse_key":"81:29","id":5829,"text":"وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلْعَـٰلَمِينَ"}]}