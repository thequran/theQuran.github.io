{"surah":"87", "verses": [{"verse_key":"87:1","id":5949,"text":" سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى"},
{"verse_key":"87:2","id":5950,"text":"ٱلَّذِى خَلَقَ فَسَوَّىٰ"},
{"verse_key":"87:3","id":5951,"text":"وَٱلَّذِى قَدَّرَ فَهَدَىٰ"},
{"verse_key":"87:4","id":5952,"text":"وَٱلَّذِىٓ أَخْرَجَ ٱلْمَرْعَىٰ"},
{"verse_key":"87:5","id":5953,"text":"فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ"},
{"verse_key":"87:6","id":5954,"text":"سَنُقْرِئُكَ فَلَا تَنسَىٰٓ"},
{"verse_key":"87:7","id":5955,"text":"إِلَّا مَا شَآءَ ٱللَّهُ ۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ"},
{"verse_key":"87:8","id":5956,"text":"وَنُيَسِّرُكَ لِلْيُسْرَىٰ"},
{"verse_key":"87:9","id":5957,"text":"فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ"},
{"verse_key":"87:10","id":5958,"text":"سَيَذَّكَّرُ مَن يَخْشَىٰ"},
{"verse_key":"87:11","id":5959,"text":"وَيَتَجَنَّبُهَا ٱلْأَشْقَى"},
{"verse_key":"87:12","id":5960,"text":"ٱلَّذِى يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ"},
{"verse_key":"87:13","id":5961,"text":"ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ"},
{"verse_key":"87:14","id":5962,"text":"قَدْ أَفْلَحَ مَن تَزَكَّىٰ"},
{"verse_key":"87:15","id":5963,"text":"وَذَكَرَ ٱسْمَ رَبِّهِۦ فَصَلَّىٰ"},
{"verse_key":"87:16","id":5964,"text":"بَلْ تُؤْثِرُونَ ٱلْحَيَوٰةَ ٱلدُّنْيَا"},
{"verse_key":"87:17","id":5965,"text":"وَٱلْـَٔاخِرَةُ خَيْرٌ وَأَبْقَىٰٓ"},
{"verse_key":"87:18","id":5966,"text":"إِنَّ هَـٰذَا لَفِى ٱلصُّحُفِ ٱلْأُولَىٰ"},
{"verse_key":"87:19","id":5967,"text":"صُحُفِ إِبْرَٰهِيمَ وَمُوسَىٰ"}]}