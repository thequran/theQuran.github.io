  "verses": [
    {
      "verse_key": "114:1",
      "id": 6231,
      "text": " قُلْ أَعُوذُ بِرَبِّ ٱلنَّاسِ"
    },
    { "verse_key": "114:2", "id": 6232, "text": "مَلِكِ ٱلنَّاسِ" },
    { "verse_key": "114:3", "id": 6233, "text": "إِلَـٰهِ ٱلنَّاسِ" },
    {
      "verse_key": "114:4",
      "id": 6234,
      "text": "مِن شَرِّ ٱلْوَسْوَاسِ ٱلْخَنَّاسِ"
    },
    {
      "verse_key": "114:5",
      "id": 6235,
      "text": "ٱلَّذِى يُوَسْوِسُ فِى صُدُورِ ٱلنَّاسِ"
    },
    { "verse_key": "114:6", "id": 6236, "text": "مِنَ ٱلْجِنَّةِ وَٱلنَّاسِ" }
  ]
