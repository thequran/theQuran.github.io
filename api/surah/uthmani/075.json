{"surah":"75", "verses": [{"verse_key":"75:1","id":5552,"text":" لَآ أُقْسِمُ بِيَوْمِ ٱلْقِيَـٰمَةِ"},
{"verse_key":"75:2","id":5553,"text":"وَلَآ أُقْسِمُ بِٱلنَّفْسِ ٱللَّوَّامَةِ"},
{"verse_key":"75:3","id":5554,"text":"أَيَحْسَبُ ٱلْإِنسَـٰنُ أَلَّن نَّجْمَعَ عِظَامَهُۥ"},
{"verse_key":"75:4","id":5555,"text":"بَلَىٰ قَـٰدِرِينَ عَلَىٰٓ أَن نُّسَوِّىَ بَنَانَهُۥ"},
{"verse_key":"75:5","id":5556,"text":"بَلْ يُرِيدُ ٱلْإِنسَـٰنُ لِيَفْجُرَ أَمَامَهُۥ"},
{"verse_key":"75:6","id":5557,"text":"يَسْـَٔلُ أَيَّانَ يَوْمُ ٱلْقِيَـٰمَةِ"},
{"verse_key":"75:7","id":5558,"text":"فَإِذَا بَرِقَ ٱلْبَصَرُ"},
{"verse_key":"75:8","id":5559,"text":"وَخَسَفَ ٱلْقَمَرُ"},
{"verse_key":"75:9","id":5560,"text":"وَجُمِعَ ٱلشَّمْسُ وَٱلْقَمَرُ"},
{"verse_key":"75:10","id":5561,"text":"يَقُولُ ٱلْإِنسَـٰنُ يَوْمَئِذٍ أَيْنَ ٱلْمَفَرُّ"},
{"verse_key":"75:11","id":5562,"text":"كَلَّا لَا وَزَرَ"},
{"verse_key":"75:12","id":5563,"text":"إِلَىٰ رَبِّكَ يَوْمَئِذٍ ٱلْمُسْتَقَرُّ"},
{"verse_key":"75:13","id":5564,"text":"يُنَبَّؤُا۟ ٱلْإِنسَـٰنُ يَوْمَئِذٍۭ بِمَا قَدَّمَ وَأَخَّرَ"},
{"verse_key":"75:14","id":5565,"text":"بَلِ ٱلْإِنسَـٰنُ عَلَىٰ نَفْسِهِۦ بَصِيرَةٌ"},
{"verse_key":"75:15","id":5566,"text":"وَلَوْ أَلْقَىٰ مَعَاذِيرَهُۥ"},
{"verse_key":"75:16","id":5567,"text":"لَا تُحَرِّكْ بِهِۦ لِسَانَكَ لِتَعْجَلَ بِهِۦٓ"},
{"verse_key":"75:17","id":5568,"text":"إِنَّ عَلَيْنَا جَمْعَهُۥ وَقُرْءَانَهُۥ"},
{"verse_key":"75:18","id":5569,"text":"فَإِذَا قَرَأْنَـٰهُ فَٱتَّبِعْ قُرْءَانَهُۥ"},
{"verse_key":"75:19","id":5570,"text":"ثُمَّ إِنَّ عَلَيْنَا بَيَانَهُۥ"},
{"verse_key":"75:20","id":5571,"text":"كَلَّا بَلْ تُحِبُّونَ ٱلْعَاجِلَةَ"},
{"verse_key":"75:21","id":5572,"text":"وَتَذَرُونَ ٱلْـَٔاخِرَةَ"},
{"verse_key":"75:22","id":5573,"text":"وُجُوهٌ يَوْمَئِذٍ نَّاضِرَةٌ"},
{"verse_key":"75:23","id":5574,"text":"إِلَىٰ رَبِّهَا نَاظِرَةٌ"},
{"verse_key":"75:24","id":5575,"text":"وَوُجُوهٌ يَوْمَئِذٍۭ بَاسِرَةٌ"},
{"verse_key":"75:25","id":5576,"text":"تَظُنُّ أَن يُفْعَلَ بِهَا فَاقِرَةٌ"},
{"verse_key":"75:26","id":5577,"text":"كَلَّآ إِذَا بَلَغَتِ ٱلتَّرَاقِىَ"},
{"verse_key":"75:27","id":5578,"text":"وَقِيلَ مَنْ ۜ رَاقٍ"},
{"verse_key":"75:28","id":5579,"text":"وَظَنَّ أَنَّهُ ٱلْفِرَاقُ"},
{"verse_key":"75:29","id":5580,"text":"وَٱلْتَفَّتِ ٱلسَّاقُ بِٱلسَّاقِ"},
{"verse_key":"75:30","id":5581,"text":"إِلَىٰ رَبِّكَ يَوْمَئِذٍ ٱلْمَسَاقُ"},
{"verse_key":"75:31","id":5582,"text":"فَلَا صَدَّقَ وَلَا صَلَّىٰ"},
{"verse_key":"75:32","id":5583,"text":"وَلَـٰكِن كَذَّبَ وَتَوَلَّىٰ"},
{"verse_key":"75:33","id":5584,"text":"ثُمَّ ذَهَبَ إِلَىٰٓ أَهْلِهِۦ يَتَمَطَّىٰٓ"},
{"verse_key":"75:34","id":5585,"text":"أَوْلَىٰ لَكَ فَأَوْلَىٰ"},
{"verse_key":"75:35","id":5586,"text":"ثُمَّ أَوْلَىٰ لَكَ فَأَوْلَىٰٓ"},
{"verse_key":"75:36","id":5587,"text":"أَيَحْسَبُ ٱلْإِنسَـٰنُ أَن يُتْرَكَ سُدًى"},
{"verse_key":"75:37","id":5588,"text":"أَلَمْ يَكُ نُطْفَةً مِّن مَّنِىٍّ يُمْنَىٰ"},
{"verse_key":"75:38","id":5589,"text":"ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوَّىٰ"},
{"verse_key":"75:39","id":5590,"text":"فَجَعَلَ مِنْهُ ٱلزَّوْجَيْنِ ٱلذَّكَرَ وَٱلْأُنثَىٰٓ"},
{"verse_key":"75:40","id":5591,"text":"أَلَيْسَ ذَٰلِكَ بِقَـٰدِرٍ عَلَىٰٓ أَن يُحْـِۧىَ ٱلْمَوْتَىٰ"}]}