{"surah":"88", "verses": [{"verse_key":"88:1","id":5968,"text":" هَلْ أَتَىٰكَ حَدِيثُ ٱلْغَـٰشِيَةِ"},
{"verse_key":"88:2","id":5969,"text":"وُجُوهٌ يَوْمَئِذٍ خَـٰشِعَةٌ"},
{"verse_key":"88:3","id":5970,"text":"عَامِلَةٌ نَّاصِبَةٌ"},
{"verse_key":"88:4","id":5971,"text":"تَصْلَىٰ نَارًا حَامِيَةً"},
{"verse_key":"88:5","id":5972,"text":"تُسْقَىٰ مِنْ عَيْنٍ ءَانِيَةٍ"},
{"verse_key":"88:6","id":5973,"text":"لَّيْسَ لَهُمْ طَعَامٌ إِلَّا مِن ضَرِيعٍ"},
{"verse_key":"88:7","id":5974,"text":"لَّا يُسْمِنُ وَلَا يُغْنِى مِن جُوعٍ"},
{"verse_key":"88:8","id":5975,"text":"وُجُوهٌ يَوْمَئِذٍ نَّاعِمَةٌ"},
{"verse_key":"88:9","id":5976,"text":"لِّسَعْيِهَا رَاضِيَةٌ"},
{"verse_key":"88:10","id":5977,"text":"فِى جَنَّةٍ عَالِيَةٍ"},
{"verse_key":"88:11","id":5978,"text":"لَّا تَسْمَعُ فِيهَا لَـٰغِيَةً"},
{"verse_key":"88:12","id":5979,"text":"فِيهَا عَيْنٌ جَارِيَةٌ"},
{"verse_key":"88:13","id":5980,"text":"فِيهَا سُرُرٌ مَّرْفُوعَةٌ"},
{"verse_key":"88:14","id":5981,"text":"وَأَكْوَابٌ مَّوْضُوعَةٌ"},
{"verse_key":"88:15","id":5982,"text":"وَنَمَارِقُ مَصْفُوفَةٌ"},
{"verse_key":"88:16","id":5983,"text":"وَزَرَابِىُّ مَبْثُوثَةٌ"},
{"verse_key":"88:17","id":5984,"text":"أَفَلَا يَنظُرُونَ إِلَى ٱلْإِبِلِ كَيْفَ خُلِقَتْ"},
{"verse_key":"88:18","id":5985,"text":"وَإِلَى ٱلسَّمَآءِ كَيْفَ رُفِعَتْ"},
{"verse_key":"88:19","id":5986,"text":"وَإِلَى ٱلْجِبَالِ كَيْفَ نُصِبَتْ"},
{"verse_key":"88:20","id":5987,"text":"وَإِلَى ٱلْأَرْضِ كَيْفَ سُطِحَتْ"},
{"verse_key":"88:21","id":5988,"text":"فَذَكِّرْ إِنَّمَآ أَنتَ مُذَكِّرٌ"},
{"verse_key":"88:22","id":5989,"text":"لَّسْتَ عَلَيْهِم بِمُصَيْطِرٍ"},
{"verse_key":"88:23","id":5990,"text":"إِلَّا مَن تَوَلَّىٰ وَكَفَرَ"},
{"verse_key":"88:24","id":5991,"text":"فَيُعَذِّبُهُ ٱللَّهُ ٱلْعَذَابَ ٱلْأَكْبَرَ"},
{"verse_key":"88:25","id":5992,"text":"إِنَّ إِلَيْنَآ إِيَابَهُمْ"},
{"verse_key":"88:26","id":5993,"text":"ثُمَّ إِنَّ عَلَيْنَا حِسَابَهُم"}]}